<?xml version='1.0' encoding='UTF-8' ?>
<portlet-app xmlns='http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd' xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance' xsi:schemaLocation='http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd' version='2.0'>
	<portlet>
		<description>Taverna</description>
		<portlet-name>Taverna</portlet-name>
		<display-name>Taverna</display-name>
		<portlet-class>com.test.WebApplication4</portlet-class>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
			<portlet-mode>EDIT</portlet-mode>
			<portlet-mode>HELP</portlet-mode>
		</supports>
		<resource-bundle>com.test.messages</resource-bundle>
		<portlet-info>
			<title>Taverna 2 Workflow Enactor</title>
			<short-title>Taverna 2 Workflow Enactor</short-title>
		</portlet-info>
	</portlet>
	<portlet>
		<description>myexp</description>
		<portlet-name>myexp</portlet-name>
		<display-name>myexp</display-name>
		<portlet-class>com.test.MyExperiment</portlet-class>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
			<portlet-mode>EDIT</portlet-mode>
			<portlet-mode>HELP</portlet-mode>
		</supports>
		<resource-bundle>messages</resource-bundle>
		<portlet-info>
			<title>myExperiment Search</title>
			<short-title>myExperiment Search</short-title>
		</portlet-info>
	</portlet>
	<portlet>
		<description>SessionManagerPortlet</description>
		<portlet-name>SessionManagerPortlet</portlet-name>
		<display-name>SessionManagerPortlet</display-name>
		<portlet-class>com.test.SessionManager</portlet-class>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
		</supports>
		<resource-bundle>com.test.messages</resource-bundle>
		<portlet-info>
			<title>Workflow Viewer</title>
			<short-title>Workflow Viewer</short-title>
		</portlet-info>
	</portlet>
	<portlet>
		<description>user</description>
		<portlet-name>user</portlet-name>
		<display-name>user</display-name>
		<portlet-class>com.test.user</portlet-class>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
		</supports>
		<resource-bundle>com.test.messages</resource-bundle>
		<portlet-info>
			<title>myExperiment</title>
			<short-title>myExperiment</short-title>
		</portlet-info>
	</portlet>
</portlet-app>
