  <body>
  
       <script type="text/javascript" src="$pmrpcUrl"></script>
       
       <script type="text/javascript">
       
         pmrpc.register( {
         	publicProcedureName : "setTitle",
         	procedure : function(title) {
         	   document.title = title;
         	   return "OK";
         	}
         });
