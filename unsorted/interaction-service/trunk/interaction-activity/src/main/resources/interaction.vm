     <iframe name="presentationFrame" height="100%" width="100%" frameborder="0" src="$presentationUrl">
</iframe>
     <iframe name="communicationFrame" height="0" width="100%" frameborder="0" src="$communicationUrl">
</iframe>
  </body>
