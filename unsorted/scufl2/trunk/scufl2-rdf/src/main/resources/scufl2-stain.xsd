<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://www.example.org/scufl2-stain/" targetNamespace="http://www.example.org/scufl2-stain/">
    <element name="workflow" type="tns:Workflow"></element>
    
    <complexType name="Workflow">
    	<sequence>
    		<element name="processors">
    			<complexType>
    				<sequence>
    					<element name="processor"
    						type="tns:Processor">
    					</element>
    				</sequence>
    			</complexType>
    		</element>
    	</sequence>
    </complexType>

    <complexType name="Processor">
    	<complexContent>
    		<extension base="tns:Named">
    			<sequence>
    				<element name="inputPorts">
    					<complexType>
    						<sequence>
    							<element name="inputPort"
    								type="tns:InputPort" maxOccurs="unbounded" minOccurs="0">
    							</element>
    						</sequence>
    					</complexType>
    				</element>
    				<element name="outputPorts">
    					<complexType>
    						<sequence>
    							<element name="outputPort"
    								type="tns:OutputPort" maxOccurs="unbounded" minOccurs="0">
    							</element>
    						</sequence>
    					</complexType>
    				</element>
    				<element name="configuration"
    					type="tns:ProcessorConfiguration" minOccurs="0" maxOccurs="unbounded">
    				</element>
    			</sequence>
    		</extension>
    	</complexContent>
    </complexType>

    <complexType name="Named">
    	<complexContent>
    		<extension base="tns:Identified">
    			<sequence>
    				<element name="name" type="string"></element>
    			</sequence>
    		</extension>
    	</complexContent>
    </complexType>

    <complexType name="Identified">
    	<sequence>
    		<element name="id" type="anyURI"></element>
    	</sequence>
    </complexType>

    <complexType name="ProcessorConfiguration"></complexType>

    <complexType name="InputPort"></complexType>

    <complexType name="OutputPort"></complexType>
</schema>