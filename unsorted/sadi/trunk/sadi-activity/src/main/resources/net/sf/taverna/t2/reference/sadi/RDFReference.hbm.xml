<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
                "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- Hibernate mapping for RDF triple -->
<hibernate-mapping>
	<joined-subclass
		name="net.sf.taverna.t2.reference.sadi.RDFReference"
		extends="net.sf.taverna.t2.reference.AbstractExternalReference">
		<!-- Link to primary key from abstract superclass -->
		<key column="bean_id" />
		<!-- Reference specific props -->
		<property name="contents" type="text" length="10000000" />
	</joined-subclass>
</hibernate-mapping>

