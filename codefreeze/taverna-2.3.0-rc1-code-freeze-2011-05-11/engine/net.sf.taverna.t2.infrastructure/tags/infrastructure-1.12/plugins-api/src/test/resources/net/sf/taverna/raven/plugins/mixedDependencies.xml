<?xml version="1.0" encoding="UTF-8"?>
<plugins:plugins xmlns:plugins="http://taverna.sf.net/2008/xml/plugins"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://taverna.sf.net/2008/xml/plugins http://taverna.sourceforge.net/2008/xml/plugins/plugins-2008-10-16.xsd ">
	<plugin>
		<!--  We'll try with elements in an unexpected order -->
		<name>workbench</name>
		<version>0.3-SNAPSHOT</version>
		<compatibility>
			<application>
				<version>2.0</version>
			</application>
		</compatibility>
		<identifier>net.sf.taverna.t2.workbench.ui.imp
		</identifier>
		<provider>taverna.sf.net</provider>
		<description>Taverna 2 Workbench</description>

		<profile>
			<artifact groupId="net.sf.taverna.t2.workbench" artifactId="file-impl"
				version="0.1-SNAPSHOT" />
			<dependency>
				<groupId>net.sf.taverna.t2.workbench
				</groupId>
				<artifactId>ui-impl</artifactId>
				<version>0.1-SNAPSHOT</version>
			</dependency>
			<artifact artifactId="saxpath" groupId="saxpath" version="1.0-FCS"
				system="true" />
			<dependency>
				<groupId>jaxen</groupId>
				<artifactId>jaxen</artifactId>
				<version>1.0-FCS</version>
				<system>true</system>
			</dependency>
		</profile>
		<repositories>
			<repository>http://www.mygrid.org.uk/maven/repository/
			</repository>
		</repositories>

	</plugin>
</plugins:plugins>
