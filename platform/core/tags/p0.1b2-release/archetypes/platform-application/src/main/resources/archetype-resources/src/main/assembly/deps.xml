#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<assembly>
	<id>dependencies</id>
	<formats>
		<format>jar</format>
	</formats>
	<includeBaseDirectory>false</includeBaseDirectory>
	<dependencySets>
		<dependencySet>
			<useProjectArtifact>false</useProjectArtifact>
			<outputDirectory></outputDirectory>
			<outputFileNameMapping></outputFileNameMapping>
			<unpack>true</unpack>
			<scope>runtime</scope>
			<unpackOptions>
				<excludes>
					<exclude>/META-INF/*.RSA</exclude>
					<exclude>/META-INF/*.DSA</exclude>
					<exclude>/META-INF/*.SF</exclude>
					<exclude>/META-INF/*.rsa</exclude>
					<exclude>/META-INF/*.dsa</exclude>
					<exclude>/META-INF/*.sf</exclude>
				</excludes>
			</unpackOptions>
		</dependencySet>
	</dependencySets>
</assembly>