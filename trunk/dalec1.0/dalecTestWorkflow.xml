<?xml version="1.0" encoding="UTF-8"?>
<s:scufl xmlns:s="http://org.embl.ebi.escience/xscufl/0.1alpha" version="0.2" log="0">
  <s:workflowdescription lsid="urn:lsid:www.mygrid.org.uk:operation:JDKBJXA2CX59" author="" title="" />
  <s:processor name="String_Constant1">
    <s:stringconstant>:</s:stringconstant>
  </s:processor>
  <s:processor name="String_Constant">
    <s:description>Dummy GFF Data</s:description>
    <s:stringconstant>null	null	0	0	0.0	.	0</s:stringconstant>
  </s:processor>
  <s:processor name="Split_string_into_string_list_by_regular_expression">
    <s:local>org.embl.ebi.escience.scuflworkers.java.SplitByRegex</s:local>
  </s:processor>
  <s:processor name="Filter_list_of_strings_by_regex">
    <s:local>org.embl.ebi.escience.scuflworkers.java.FilterStringList</s:local>
  </s:processor>
  <s:processor name="Concatenate_two_strings">
    <s:local>org.embl.ebi.escience.scuflworkers.java.StringConcat</s:local>
  </s:processor>
  <s:processor name="String_Constant2">
    <s:stringconstant>.*[A-Z0-9].*</s:stringconstant>
  </s:processor>
  <s:processor name="Merge_string_list_to_string">
    <s:local>org.embl.ebi.escience.scuflworkers.java.StringListMerge</s:local>
  </s:processor>
  <s:processor name="seqret">
    <s:description>Reads and writes (returns) sequences</s:description>
    <s:soaplabwsdl>http://www.ebi.ac.uk/soaplab/services/edit.seqret</s:soaplabwsdl>
  </s:processor>
  <s:link source="seqID" sink="Split_string_into_string_list_by_regular_expression:string" />
  <s:link source="String_Constant1:value" sink="Split_string_into_string_list_by_regular_expression:regex" />
  <s:link source="Split_string_into_string_list_by_regular_expression:split" sink="Filter_list_of_strings_by_regex:stringlist" />
  <s:link source="String_Constant2:value" sink="Filter_list_of_strings_by_regex:regex" />
  <s:link source="seqID" sink="seqret:sequence_usa" />
  <s:link source="seqret:outseq" sink="Raw Sequence" />
  <s:link source="Filter_list_of_strings_by_regex:filteredlist" sink="Merge_string_list_to_string:stringlist" />
  <s:link source="Merge_string_list_to_string:concatenated" sink="Concatenate_two_strings:string1" />
  <s:link source="String_Constant:value" sink="Concatenate_two_strings:string2" />
  <s:link source="Concatenate_two_strings:output" sink="annotations" />
  <s:source name="seqID" />
  <s:sink name="Raw Sequence" />
  <s:sink name="annotations" />
</s:scufl>


