<page title="Simple example 2">
  <header>
    <require keys="user"/>
  </header>
  <contents>
    <grid name="Name" icon="users" cols="1">
      <script>
        user.getName()
      </script>
    </grid>
    <grid name="Phone" icon="users" cols="1">
      <script>
        user.getPhone()
      </script>
    </grid>
    <grid name="Country" icon="users" cols="1">
      <script>
        user.getCountry()
      </script>
    </grid>
  </contents>	
</page>