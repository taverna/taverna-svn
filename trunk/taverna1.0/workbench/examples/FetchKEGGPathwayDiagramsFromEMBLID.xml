<?xml version="1.0" encoding="UTF-8"?>
<s:scufl xmlns:s="http://org.embl.ebi.escience/xscufl/0.1alpha" version="0.1" log="0">
  <s:processor name="keggorg">
    <s:stringconstant maxretries="0" retrydelay="0" retrybackoff="1.0">hsa</s:stringconstant>
  </s:processor>
  <s:processor name="emblAccNumber">
    <s:stringconstant maxretries="0" retrydelay="0" retrybackoff="1.0">embl-AccNumber:AA326664</s:stringconstant>
  </s:processor>
  <s:processor name="getPathwayDiagrams">
    <s:local maxretries="0" retrydelay="0" retrybackoff="1.0">org.embl.ebi.escience.scuflworkers.java.WebImageFetcher</s:local>
  </s:processor>
  <s:processor name="getHsaIds">
    <s:arbitrarywsdl maxretries="0" retrydelay="0" retrybackoff="1.0">
      <s:wsdl>http://mygrid.ncl.ac.uk/axis/services/keggmapper?wsdl</s:wsdl>
      <s:operation>getHsaIds</s:operation>
    </s:arbitrarywsdl>
  </s:processor>
  <s:processor name="getPathways">
    <s:arbitrarywsdl maxretries="0" retrydelay="0" retrybackoff="1.0">
      <s:wsdl>http://soap.genome.ad.jp/KEGG.wsdl</s:wsdl>
      <s:operation>mark_all_pathways_by_genes</s:operation>
    </s:arbitrarywsdl>
  </s:processor>
  <s:processor name="getKeggEntry">
    <s:arbitrarywsdl maxretries="0" retrydelay="0" retrybackoff="1.0">
      <s:wsdl>http://soap.genome.ad.jp/KEGG.wsdl</s:wsdl>
      <s:operation>get_entries</s:operation>
    </s:arbitrarywsdl>
  </s:processor>
  <s:link>
    <s:input>getPathways:oids</s:input>
    <s:output>getHsaIds:getHsaIdsReturn</s:output>
  </s:link>
  <s:link>
    <s:input>getPathways:abbr</s:input>
    <s:output>keggorg:value</s:output>
  </s:link>
  <s:link>
    <s:input>getKeggEntry:kids</s:input>
    <s:output>getHsaIds:getHsaIdsReturn</s:output>
  </s:link>
  <s:link>
    <s:input>getHsaIds:query</s:input>
    <s:output>emblAccNumber:value</s:output>
  </s:link>
  <s:link>
    <s:input>pathwayDiagrams</s:input>
    <s:output>getPathwayDiagrams:image</s:output>
  </s:link>
  <s:link>
    <s:input>keggGeneEntries</s:input>
    <s:output>getKeggEntry:return</s:output>
  </s:link>
  <s:link>
    <s:input>getPathwayDiagrams:url</s:input>
    <s:output>getPathways:return</s:output>
  </s:link>
  <s:sink>
    pathwayDiagrams
    <s:metadata>
      <s:mimeTypes>
        <s:mimeType>image/gif</s:mimeType>
      </s:mimeTypes>
      <s:description />
      <s:semanticType />
    </s:metadata>
  </s:sink>
  <s:sink>
    keggGeneEntries
    <s:metadata>
      <s:mimeTypes />
      <s:description />
      <s:semanticType />
    </s:metadata>
  </s:sink>
</s:scufl>


