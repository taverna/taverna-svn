<?xml version="1.0" encoding="UTF-8"?>
<s:scufl xmlns:s="http://org.embl.ebi.escience/xscufl/0.1alpha" version="0.1" log="0">
  <s:processor name="getJointAncestry">
    <s:local>org.embl.ebi.escience.scuflworkers.java.StringSetIntersection</s:local>
  </s:processor>
  <s:processor name="intersectionColour">
    <s:stringconstant>darkseagreen3</s:stringconstant>
  </s:processor>
  <s:processor name="term1">
    <s:stringconstant>GO:0007601</s:stringconstant>
  </s:processor>
  <s:processor name="term2">
    <s:stringconstant>GO:0007607</s:stringconstant>
  </s:processor>
  <s:processor name="addTerm1">
    <s:arbitrarywsdl>
      <s:wsdl>http://www.ebi.ac.uk/collab/mygrid/service1/goviz/GoViz.jws?wsdl</s:wsdl>
      <s:operation>addTerm</s:operation>
    </s:arbitrarywsdl>
  </s:processor>
  <s:processor name="getAncestorsForTerm2">
    <s:arbitrarywsdl>
      <s:wsdl>http://www.ebi.ac.uk/collab/mygrid/service1/goviz/GoViz.jws?wsdl</s:wsdl>
      <s:operation>getAncestors</s:operation>
    </s:arbitrarywsdl>
  </s:processor>
  <s:processor name="getAncestorsForTerm1">
    <s:arbitrarywsdl>
      <s:wsdl>http://www.ebi.ac.uk/collab/mygrid/service1/goviz/GoViz.jws?wsdl</s:wsdl>
      <s:operation>getAncestors</s:operation>
    </s:arbitrarywsdl>
  </s:processor>
  <s:processor name="showIntersection">
    <s:arbitrarywsdl>
      <s:wsdl>http://www.ebi.ac.uk/collab/mygrid/service1/goviz/GoViz.jws?wsdl</s:wsdl>
      <s:operation>markTerm</s:operation>
    </s:arbitrarywsdl>
  </s:processor>
  <s:processor name="addTerm2">
    <s:arbitrarywsdl>
      <s:wsdl>http://www.ebi.ac.uk/collab/mygrid/service1/goviz/GoViz.jws?wsdl</s:wsdl>
      <s:operation>addTerm</s:operation>
    </s:arbitrarywsdl>
  </s:processor>
  <s:processor name="createVizSession">
    <s:arbitrarywsdl>
      <s:wsdl>http://www.ebi.ac.uk/collab/mygrid/service1/goviz/GoViz.jws?wsdl</s:wsdl>
      <s:operation>createSession</s:operation>
    </s:arbitrarywsdl>
  </s:processor>
  <s:processor name="destroyVizSession">
    <s:arbitrarywsdl>
      <s:wsdl>http://www.ebi.ac.uk/collab/mygrid/service1/goviz/GoViz.jws?wsdl</s:wsdl>
      <s:operation>destroySession</s:operation>
    </s:arbitrarywsdl>
  </s:processor>
  <s:processor name="getDiagram">
    <s:arbitrarywsdl>
      <s:wsdl>http://www.ebi.ac.uk/collab/mygrid/service1/goviz/GoViz.jws?wsdl</s:wsdl>
      <s:operation>getDot</s:operation>
    </s:arbitrarywsdl>
  </s:processor>
  <s:processor name="colourTerm1Ancestors">
    <s:arbitrarywsdl>
      <s:wsdl>http://www.ebi.ac.uk/collab/mygrid/service1/goviz/GoViz.jws?wsdl</s:wsdl>
      <s:operation>markTerm</s:operation>
    </s:arbitrarywsdl>
  </s:processor>
  <s:processor name="Term1AncestorsColour">
    <s:stringconstant>darkslategray3</s:stringconstant>
  </s:processor>
  <s:link>
    <s:input>getJointAncestry:list2</s:input>
    <s:output>getAncestorsForTerm2:getAncestorsReturn</s:output>
  </s:link>
  <s:link>
    <s:input>getJointAncestry:list1</s:input>
    <s:output>getAncestorsForTerm1:getAncestorsReturn</s:output>
  </s:link>
  <s:link>
    <s:input>addTerm1:sessionID</s:input>
    <s:output>createVizSession:createSessionReturn</s:output>
  </s:link>
  <s:link>
    <s:input>addTerm2:sessionID</s:input>
    <s:output>createVizSession:createSessionReturn</s:output>
  </s:link>
  <s:link>
    <s:input>showIntersection:colour</s:input>
    <s:output>intersectionColour:value</s:output>
  </s:link>
  <s:link>
    <s:input>showIntersection:geneOntologyID</s:input>
    <s:output>getJointAncestry:intersection</s:output>
  </s:link>
  <s:link>
    <s:input>showIntersection:sessionID</s:input>
    <s:output>createVizSession:createSessionReturn</s:output>
  </s:link>
  <s:link>
    <s:input>getDiagram:sessionID</s:input>
    <s:output>createVizSession:createSessionReturn</s:output>
  </s:link>
  <s:link>
    <s:input>diagram</s:input>
    <s:output>getDiagram:getDotReturn</s:output>
  </s:link>
  <s:link>
    <s:input>destroyVizSession:sessionID</s:input>
    <s:output>createVizSession:createSessionReturn</s:output>
  </s:link>
  <s:link>
    <s:input>addTerm1:geneOntologyID</s:input>
    <s:output>term1:value</s:output>
  </s:link>
  <s:link>
    <s:input>getAncestorsForTerm1:geneOntologyID</s:input>
    <s:output>term1:value</s:output>
  </s:link>
  <s:link>
    <s:input>getAncestorsForTerm2:geneOntologyID</s:input>
    <s:output>term2:value</s:output>
  </s:link>
  <s:link>
    <s:input>addTerm2:geneOntologyID</s:input>
    <s:output>term2:value</s:output>
  </s:link>
  <s:link>
    <s:input>colourTerm1Ancestors:geneOntologyID</s:input>
    <s:output>getAncestorsForTerm1:getAncestorsReturn</s:output>
  </s:link>
  <s:link>
    <s:input>colourTerm1Ancestors:sessionID</s:input>
    <s:output>createVizSession:createSessionReturn</s:output>
  </s:link>
  <s:link>
    <s:input>colourTerm1Ancestors:colour</s:input>
    <s:output>Term1AncestorsColour:value</s:output>
  </s:link>
  <s:sink>
    diagram
    <s:metadata>
      <s:mimeTypes>
        <s:mimeType>text/x-graphviz</s:mimeType>
      </s:mimeTypes>
      <s:description />
      <s:semanticType />
    </s:metadata>
  </s:sink>
  <s:coordination name="showIntersection_BLOCKON_addTerm2">
    <s:condition>
      <s:state>Completed</s:state>
      <s:target>addTerm2</s:target>
    </s:condition>
    <s:action>
      <s:target>showIntersection</s:target>
      <s:statechange>
        <s:from>Scheduled</s:from>
        <s:to>Running</s:to>
      </s:statechange>
    </s:action>
  </s:coordination>
  <s:coordination name="getDiagram_BLOCKON_showIntersection">
    <s:condition>
      <s:state>Completed</s:state>
      <s:target>showIntersection</s:target>
    </s:condition>
    <s:action>
      <s:target>getDiagram</s:target>
      <s:statechange>
        <s:from>Scheduled</s:from>
        <s:to>Running</s:to>
      </s:statechange>
    </s:action>
  </s:coordination>
  <s:coordination name="destroyVizSession_BLOCKON_getDiagram">
    <s:condition>
      <s:state>Completed</s:state>
      <s:target>getDiagram</s:target>
    </s:condition>
    <s:action>
      <s:target>destroyVizSession</s:target>
      <s:statechange>
        <s:from>Scheduled</s:from>
        <s:to>Running</s:to>
      </s:statechange>
    </s:action>
  </s:coordination>
  <s:coordination name="colourTerm1Ancestors_BLOCKON_addTerm1">
    <s:condition>
      <s:state>Completed</s:state>
      <s:target>addTerm1</s:target>
    </s:condition>
    <s:action>
      <s:target>colourTerm1Ancestors</s:target>
      <s:statechange>
        <s:from>Scheduled</s:from>
        <s:to>Running</s:to>
      </s:statechange>
    </s:action>
  </s:coordination>
  <s:coordination name="showIntersection_BLOCKON_colourTerm1Ancestors">
    <s:condition>
      <s:state>Completed</s:state>
      <s:target>colourTerm1Ancestors</s:target>
    </s:condition>
    <s:action>
      <s:target>showIntersection</s:target>
      <s:statechange>
        <s:from>Scheduled</s:from>
        <s:to>Running</s:to>
      </s:statechange>
    </s:action>
  </s:coordination>
</s:scufl>


