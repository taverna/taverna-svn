<?xml version="1.0" encoding="UTF-8"?>
<s:scufl xmlns:s="http://org.embl.ebi.escience/xscufl/0.1alpha" version="0.1" log="0">
  <s:processor name="databaseid">
    <s:stringconstant>MEDLINE</s:stringconstant>
  </s:processor>
  <s:processor name="srsebi_getmedlineid">
    <s:arbitrarywsdl>
      <s:wsdl>http://mygrid.ncl.ac.uk/axis/services/SrsEbiQuery?wsdl</s:wsdl>
      <s:operation>queryByXRef</s:operation>
    </s:arbitrarywsdl>
  </s:processor>
  <s:processor name="srsebi_getmedlinerecords">
    <s:arbitrarywsdl>
      <s:wsdl>http://mygrid.ncl.ac.uk/axis/services/SrsEbiQuery?wsdl</s:wsdl>
      <s:operation>queryByArrayIds</s:operation>
    </s:arbitrarywsdl>
  </s:processor>
  <s:processor name="affymapper_getemblaccnumber">
    <s:arbitrarywsdl>
      <s:wsdl>http://mygrid.ncl.ac.uk/axis/services/affymapper?wsdl</s:wsdl>
      <s:operation>getAccessionNumber</s:operation>
    </s:arbitrarywsdl>
  </s:processor>
  <s:processor name="ProbeSetID">
    <s:stringconstant>154_at</s:stringconstant>
  </s:processor>
  <s:link>
    <s:input>srsebi_getmedlineid:usa</s:input>
    <s:output>affymapper_getemblaccnumber:getAccessionNumberReturn</s:output>
  </s:link>
  <s:link>
    <s:input>srsebi_getmedlineid:XRefDatabankId</s:input>
    <s:output>databaseid:value</s:output>
  </s:link>
  <s:link>
    <s:input>srsebi_getmedlinerecords:usas</s:input>
    <s:output>srsebi_getmedlineid:queryByXRefReturn</s:output>
  </s:link>
  <s:link>
    <s:input>medlinerecords</s:input>
    <s:output>srsebi_getmedlinerecords:queryByArrayIdsReturn</s:output>
  </s:link>
  <s:link>
    <s:input>affymapper_getemblaccnumber:probeSetId</s:input>
    <s:output>ProbeSetID:value</s:output>
  </s:link>
  <s:sink>
    medlinerecords
    <s:metadata>
      <s:mimeTypes>
        <s:mimeType>text/xml</s:mimeType>
      </s:mimeTypes>
      <s:description />
      <s:semanticType />
    </s:metadata>
  </s:sink>
</s:scufl>


