<?xml version="1.0" encoding="UTF-8"?>
<s:scufl xmlns:s="http://org.embl.ebi.escience/xscufl/0.1alpha" version="0.1" log="0">
  <s:processor name="seqret1">
    <s:description>Reads and writes (returns) sequences</s:description>
    <s:soaplabwsdl>http://industry.ebi.ac.uk/soap/soaplab/edit::seqret</s:soaplabwsdl>
  </s:processor>
  <s:processor name="emma">
    <s:description>Multiple alignment program - interface to ClustalW program</s:description>
    <s:soaplabwsdl>http://industry.ebi.ac.uk/soap/soaplab/alignment_multiple::emma</s:soaplabwsdl>
  </s:processor>
  <s:processor name="plot">
    <s:description>Displays aligned sequences, with colouring and boxing</s:description>
    <s:soaplabwsdl>http://industry.ebi.ac.uk/soap/soaplab/alignment_multiple::prettyplot</s:soaplabwsdl>
  </s:processor>
  <s:processor name="sequenceid">
    <s:stringconstant>swallid:ops2_*</s:stringconstant>
  </s:processor>
  <s:processor name="prophecy">
    <s:description>Creates matrices/profiles from multiple alignments</s:description>
    <s:soaplabwsdl>http://industry.ebi.ac.uk/soap/soaplab/nucleic_profiles::prophecy</s:soaplabwsdl>
  </s:processor>
  <s:processor name="prophecyType">
    <s:stringconstant>G</s:stringconstant>
  </s:processor>
  <s:processor name="prophecyName">
    <s:stringconstant>Ops2 Sequences</s:stringconstant>
  </s:processor>
  <s:processor name="prophet">
    <s:description>Gapped alignment for profiles</s:description>
    <s:soaplabwsdl>http://industry.ebi.ac.uk/soap/soaplab/nucleic_profiles::prophet</s:soaplabwsdl>
  </s:processor>
  <s:processor name="transeq">
    <s:description>Translate nucleic acid sequences</s:description>
    <s:soaplabwsdl>http://industry.ebi.ac.uk/soap/soaplab/nucleic_translation::transeq</s:soaplabwsdl>
  </s:processor>
  <s:processor name="transeqSequenceID">
    <s:stringconstant>embl:xlrhodop</s:stringconstant>
  </s:processor>
  <s:processor name="sbegin">
    <s:stringconstant>110</s:stringconstant>
  </s:processor>
  <s:processor name="send">
    <s:stringconstant>1171</s:stringconstant>
  </s:processor>
  <s:link>
    <s:input>seqret1:sequence_usa</s:input>
    <s:output>sequenceid:value</s:output>
  </s:link>
  <s:link>
    <s:input>emma:inseqs_direct_data</s:input>
    <s:output>seqret1:outseq</s:output>
  </s:link>
  <s:link>
    <s:input>plot:msf_direct_data</s:input>
    <s:output>emma:outseq</s:output>
  </s:link>
  <s:link>
    <s:input>outputPlot</s:input>
    <s:output>plot:Graphics_in_PNG</s:output>
  </s:link>
  <s:link>
    <s:input>prophecy:sequence_direct_data</s:input>
    <s:output>emma:outseq</s:output>
  </s:link>
  <s:link>
    <s:input>prophecy:type</s:input>
    <s:output>prophecyType:value</s:output>
  </s:link>
  <s:link>
    <s:input>prophecy:name</s:input>
    <s:output>prophecyName:value</s:output>
  </s:link>
  <s:link>
    <s:input>prophet:infile_direct_data</s:input>
    <s:output>prophecy:outf</s:output>
  </s:link>
  <s:link>
    <s:input>prophet:sequence_direct_data</s:input>
    <s:output>transeq:outseq</s:output>
  </s:link>
  <s:link>
    <s:input>transeq:sequence_usa</s:input>
    <s:output>transeqSequenceID:value</s:output>
  </s:link>
  <s:link>
    <s:input>transeq:sbegin</s:input>
    <s:output>sbegin:value</s:output>
  </s:link>
  <s:link>
    <s:input>transeq:send</s:input>
    <s:output>send:value</s:output>
  </s:link>
  <s:link>
    <s:input>prophetOutput</s:input>
    <s:output>prophet:outfile</s:output>
  </s:link>
  <s:sink>
    outputPlot
    <s:metadata>
      <s:mimeTypes>
        <s:mimeType>image/png</s:mimeType>
      </s:mimeTypes>
      <s:description>The array of png images returned from the plot processor</s:description>
      <s:semanticType>http://www.mygrid.org.uk/ontology#domain_concept</s:semanticType>
    </s:metadata>
  </s:sink>
  <s:sink>
    prophetOutput
    <s:metadata>
      <s:mimeTypes />
      <s:description />
      <s:semanticType />
    </s:metadata>
  </s:sink>
</s:scufl>


