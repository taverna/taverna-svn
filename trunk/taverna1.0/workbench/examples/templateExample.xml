<?xml version="1.0" encoding="UTF-8"?>
<s:scufl xmlns:s="http://org.embl.ebi.escience/xscufl/0.1alpha" version="0.1" log="0">
  <s:processor name="concatenate">
    <s:local maxretries="0" retrydelay="0" retrybackoff="1.0">org.embl.ebi.escience.scuflworkers.java.StringConcat</s:local>
	<s:template>
		<s:templatePortReference name="output"/>
		<s:templateLiteral> is the concatenation of </s:templateLiteral>
		<s:templatePortReference name="string1"/>
		<s:templateLiteral> and </s:templateLiteral>
		<s:templatePortReference name="string2"/>
	</s:template>
  </s:processor>
  <s:link>
    <s:input>concatenate:string1</s:input>
    <s:output>string1</s:output>
  </s:link>
  <s:link>
    <s:input>concatenate:string2</s:input>
    <s:output>string2</s:output>
  </s:link>
  <s:link>
    <s:input>concatenation</s:input>
    <s:output>concatenate:output</s:output>
  </s:link>
  <s:source>
    string1
    <s:metadata>
      <s:mimeTypes />
      <s:description />
      <s:semanticType />
    </s:metadata>
  </s:source>
  <s:source>
    string2
    <s:metadata>
      <s:mimeTypes />
      <s:description />
      <s:semanticType />
    </s:metadata>
  </s:source>
  <s:sink>
    concatenation
    <s:metadata>
      <s:mimeTypes />
      <s:description />
      <s:semanticType />
    </s:metadata>
  </s:sink>
</s:scufl>


