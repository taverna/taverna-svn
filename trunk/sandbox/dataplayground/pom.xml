<?xml version="1.0" encoding="UTF-8"?><project>
  <modelVersion>4.0.0</modelVersion>
  <groupId>uk.org.mygrid.dataplayground</groupId>
  <artifactId>dataplayground</artifactId>
  <version>0.0.1</version>
  <description></description>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>1.5</source>
          <target>1.5</target>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-surefire-plugin</artifactId>
        <configuration>
          <skip>false</skip>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-assembly-plugin</artifactId>
        <version>2.2-svn-SNAPSHOT</version>
        <configuration>
          <descriptorRefs>
            <descriptorRef>jar-with-dependencies</descriptorRef>
          </descriptorRefs>
        </configuration>
      </plugin>
    </plugins>
  </build>
  <profiles>
    <profile>
      <id>default-tools.jar</id>
      <activation>
        <property>
          <name>java.vendor</name>
          <value>Sun Microsystems Inc.</value>
        </property>
      </activation>
      <dependencies>
        <dependency>
          <groupId>com.sun</groupId>
          <artifactId>tools</artifactId>
          <version>1.4.2</version>
          <scope>system</scope>
          <systemPath>C:/Program Files/Java/jdk1.5.0_07/lib/tools.jar</systemPath>
        </dependency>
      </dependencies>
    </profile>
  </profiles>
  <repositories>
    <repository>
      <releases>
        <enabled>false</enabled>
      </releases>
      <snapshots />
      <id>mygrid-snapshot-repository</id>
      <name>myGrid Snapshot Repository</name>
      <url>http://www.mygrid.org.uk/maven/snapshot-repository</url>
    </repository>
    <repository>
      <releases />
      <snapshots>
        <enabled>false</enabled>
      </snapshots>
      <id>mygrid-repository</id>
      <name>myGrid Repository</name>
      <url>http://www.mygrid.org.uk/maven/repository</url>
    </repository>
    <repository>
      <releases />
      <snapshots>
        <enabled>false</enabled>
      </snapshots>
      <id>mobycentral</id>
      <name>MobyCentral Repository</name>
      <url>http://mobycentral.icapture.ubc.ca/maven</url>
    </repository>
  </repositories>
  <pluginRepositories>
    <pluginRepository>
      <releases>
        <enabled>false</enabled>
      </releases>
      <snapshots />
      <id>Maven Snapshots</id>
      <url>http://snapshots.maven.codehaus.org/maven2/</url>
    </pluginRepository>
    <pluginRepository>
      <releases>
        <enabled>false</enabled>
      </releases>
      <snapshots />
      <id>mygrid-snapshot-repository</id>
      <name>myGrid Snapshot Repository</name>
      <url>http://www.mygrid.org.uk/maven/snapshot-repository</url>
    </pluginRepository>
    <pluginRepository>
      <releases />
      <snapshots>
        <enabled>false</enabled>
      </snapshots>
      <id>mygrid-repository</id>
      <name>myGrid Repository</name>
      <url>http://www.mygrid.org.uk/maven/repository</url>
    </pluginRepository>
  </pluginRepositories>
  <dependencies>
    <dependency>
      <groupId>uk.org.mygrid.taverna.scufl</groupId>
      <artifactId>scufl-ui-api</artifactId>
      <version>1.5.1</version>
    </dependency>
    <dependency>
      <groupId>uk.org.mygrid.taverna.scufl</groupId>
      <artifactId>scufl-ui-api</artifactId>
      <version>1.5.1</version>
    </dependency>
    <dependency>
      <groupId>biomoby.org</groupId>
      <artifactId>jmoby</artifactId>
      <version>1</version>
    </dependency>
    <dependency>
      <groupId>uk.org.mygrid.taverna.perspectives</groupId>
      <artifactId>perspective-core</artifactId>
      <version>1.5.1</version>
    </dependency>
    <dependency>
      <groupId>colt</groupId>
      <artifactId>colt</artifactId>
      <version>1.2.0</version>
    </dependency>
    <dependency>
      <groupId>concurrent</groupId>
      <artifactId>concurrent</artifactId>
      <version>1.3.4</version>
    </dependency>
    <dependency>
      <groupId>commons-collections</groupId>
      <artifactId>commons-collections</artifactId>
      <version>3.1</version>
    </dependency>
    <dependency>
      <groupId>jung</groupId>
      <artifactId>jung</artifactId>
      <version>1.7.6</version>
    </dependency>
    <dependency>
      <groupId>uk.org.mygrid.taverna</groupId>
      <artifactId>taverna-biomoby</artifactId>
      <version>1.5.1</version>
    </dependency>
    <dependency>
      <groupId>uk.org.mygrid.taverna</groupId>
      <artifactId>taverna-scavenger-ui</artifactId>
      <version>1.5.1</version>
    </dependency>
    <dependency>
      <groupId>uk.org.mygrid.taverna.scufl</groupId>
      <artifactId>scufl-tools</artifactId>
      <version>1.5.1</version>
    </dependency>
  </dependencies>
  <distributionManagement>
    <repository>
      <id>local</id>
      <name>local</name>
      <url>file:///C:/Documents and Settings/Matt/Application Data/Taverna/repository/</url>
    </repository>
  </distributionManagement>
</project>