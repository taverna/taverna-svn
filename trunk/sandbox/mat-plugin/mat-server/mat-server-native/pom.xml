<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>net.sf.taverna.matlabactivity.matserver</groupId>
        <artifactId>mat-server</artifactId>
        <version>1.0-SNAPSHOT</version>
    </parent>
    <groupId>net.sf.taverna.matlabactivity.matserver</groupId>
    <artifactId>mat-server-native</artifactId>
    <name>MatServer native</name>
    <version>1.0-SNAPSHOT</version>
    <packaging>pom</packaging>
    <modules>
        <module>linux</module>
        <!--module>macosx</module-->
        <module>win32</module>
    </modules>
    <build>
        <directory>../../target</directory>
        <outputDirectory>../target</outputDirectory>
        <testOutputDirectory>../target</testOutputDirectory>
    </build>
    <profiles>
        <profile>
            <id>win32</id>
            <activation>
                <property>
                    <name>platform</name>
                    <value>win32</value>
                </property>
            </activation>
            <modules>
                <module>win32</module>
            </modules>
        </profile>
        <profile>
            <id>linux</id>
            <activation>
                <property>
                    <name>platform</name>
                    <value>linux</value>
                </property>
            </activation>
            <modules>
                <module>linux</module>
            </modules>
        </profile>
        <!--profile>
            <id>macosx</id>
            <activation>
                <property>
                    <name>platform</name>
                    <value>macosx</value>
                </property>
            </activation>
            <modules>
                <module>macosx</module>
            </modules>
        </profile-->
    </profiles>
    <properties>
        <netbeans.hint.useExternalMaven>true</netbeans.hint.useExternalMaven>
    </properties>
</project>
