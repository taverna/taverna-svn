<?xml version="1.0" encoding="UTF-8"?>
<s:scufl xmlns:s="http://org.embl.ebi.escience/xscufl/0.1alpha" version="0.2" log="0">
  <s:workflowdescription lsid="urn:lsid:net.sf.taverna:wfDefinition:2d865bef-19e2-45a2-8338-abd0d93bbb77" author="" title="lists_iterate" />
  <s:processor name="Concatenate_two_strings1">
    <s:defaults>
      <s:default name="string2">XXX</s:default>
    </s:defaults>
    <s:local>org.embl.ebi.escience.scuflworkers.java.StringConcat</s:local>
  </s:processor>
  <s:processor name="Concatenate_two_strings2">
    <s:defaults>
      <s:default name="string2">sdfds</s:default>
    </s:defaults>
    <s:local>org.embl.ebi.escience.scuflworkers.java.StringConcat</s:local>
  </s:processor>
  <s:processor name="Concatenate_two_strings">
    <s:defaults>
      <s:default name="string2">,boo</s:default>
    </s:defaults>
    <s:local>org.embl.ebi.escience.scuflworkers.java.StringConcat</s:local>
  </s:processor>
  <s:processor name="List_Emitter">
    <s:beanshell>
      <s:scriptvalue>List list = new ArrayList();
int icount = Integer.parseInt(count);

for (int i=0;i&lt;icount;i++) {
	list.add(String.valueOf(i));
}</s:scriptvalue>
      <s:beanshellinputlist>
        <s:beanshellinput s:syntactictype="'text/plain'">count</s:beanshellinput>
      </s:beanshellinputlist>
      <s:beanshelloutputlist>
        <s:beanshelloutput s:syntactictype="l('text/plain')">list</s:beanshelloutput>
      </s:beanshelloutputlist>
      <s:dependencies s:classloader="iteration" />
    </s:beanshell>
  </s:processor>
  <s:processor name="Concat">
    <s:beanshell>
      <s:scriptvalue>String output = input + "XXX"</s:scriptvalue>
      <s:beanshellinputlist>
        <s:beanshellinput s:syntactictype="'text/plain'">input</s:beanshellinput>
      </s:beanshellinputlist>
      <s:beanshelloutputlist>
        <s:beanshelloutput s:syntactictype="'text/plain'">output</s:beanshelloutput>
      </s:beanshelloutputlist>
      <s:dependencies s:classloader="iteration" />
    </s:beanshell>
  </s:processor>
  <s:link source="Concat:output" sink="Concatenate_two_strings:string1" />
  <s:link source="Concatenate_two_strings1:output" sink="Concatenate_two_strings2:string1" />
  <s:link source="Concatenate_two_strings:output" sink="Concatenate_two_strings1:string1" />
  <s:link source="List_Emitter:list" sink="Concat:input" />
  <s:link source="count" sink="List_Emitter:count" />
  <s:link source="Concatenate_two_strings2:output" sink="out" />
  <s:source name="count" />
  <s:sink name="out">
    <s:metadata>
      <s:mimeTypes>
        <s:mimeType>l('text/plain')</s:mimeType>
        <s:mimeType>'text/plain'</s:mimeType>
      </s:mimeTypes>
    </s:metadata>
  </s:sink>
</s:scufl>

