<plugins>
	<plugin>
		<name>workbench</name>
		<description>Taverna 2 Workbench</description>
		<identifier>net.sf.taverna.t2.workbench.ui.impl</identifier>
		<version>0.1-SNAPSHOT</version>
		<provider>taverna.sf.net</provider>
		<enabled>true</enabled>
		<repositories>
			<repository>file:///Users/stain/.m2/repository/</repository>
			<repository>
				http://www.mygrid.org.uk/maven/repository/
			</repository>
			<repository>
				http://www.mygrid.org.uk/maven/snapshot-repository/
			</repository>
			<repository>http://mirrors.sunsite.dk/maven2/</repository>
			<repository>http://www.ibiblio.org/maven2/</repository>
		</repositories>
		<profile>
			<artifact groupId="net.sf.taverna.t2.workbench"
				artifactId="ui-impl" version="0.1-SNAPSHOT" />
			<artifact groupId="net.sf.taverna.t2.workbench"
				artifactId="file-impl" version="0.1-SNAPSHOT" />
			<artifact groupId="net.sf.taverna.t2.workbench"
				artifactId="menu-impl" version="0.1-SNAPSHOT" />
			<artifact groupId="net.sf.taverna.t2.workbench"
				artifactId="contextual-views" version="0.1-SNAPSHOT" />
			<artifact groupId="net.sf.taverna.t2.workbench.views"
				artifactId="graph" version="0.0.1-SNAPSHOT" />

			<artifact groupId="net.sf.taverna.t2.workbench"
				artifactId="perspective-design" version="1.7-SNAPSHOT" />


		</profile>
		<application>
			<version>2.0</version>
		</application>
	</plugin>
