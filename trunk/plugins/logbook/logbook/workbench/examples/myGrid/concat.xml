<?xml version="1.0" encoding="UTF-8"?>
<s:scufl xmlns:s="http://org.embl.ebi.escience/xscufl/0.1alpha" version="0.2" log="0">
  <s:workflowdescription lsid="urn:lsid:www.mygrid.org.uk:operation:Z82W8BBCJD20" author="Daniele Turi" title="Concatenates input and subworkflow">Concatenates input and subworkflow</s:workflowdescription>
  <s:processor name="c" boring="true">
    <s:stringconstant>c</s:stringconstant>
  </s:processor>
  <s:processor name="cat">
    <s:local>org.embl.ebi.escience.scuflworkers.java.StringConcat</s:local>
  </s:processor>
  <s:processor name="a" boring="true">
    <s:stringconstant>a</s:stringconstant>
  </s:processor>
  <s:processor name="NestedWorkflow">
    <s:workflow>
      <s:scufl version="0.2" log="0">
        <s:workflowdescription lsid="urn:lsid:www.mygrid.org.uk:operation:Z82W8BBCJD0" author="Daniele Turi" title="Concatenates input and constant">Concatenates input and constant</s:workflowdescription>
        <s:processor name="subcat">
          <s:local>org.embl.ebi.escience.scuflworkers.java.StringConcat</s:local>
        </s:processor>
        <s:processor name="t" boring="true">
          <s:stringconstant>t</s:stringconstant>
        </s:processor>
        <s:link source="sbin" sink="subcat:string1" />
        <s:link source="t:value" sink="subcat:string2" />
        <s:link source="subcat:output" sink="sbout" />
        <s:source name="sbin" />
        <s:sink name="sbout" />
      </s:scufl>
    </s:workflow>
  </s:processor>
  <s:link source="NestedWorkflow:sbout" sink="cat:string2" />
  <s:link source="a:value" sink="NestedWorkflow:sbin" />
  <s:link source="c:value" sink="cat:string1" />
  <s:link source="cat:output" sink="out" />
  <s:sink name="out" />
</s:scufl>


