<?xml version="1.0" encoding="UTF-8"?>
<project>
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>uk.org.mygrid.provenance</groupId>
		<artifactId>logbook-parent</artifactId>
		<version>1.2</version>
	</parent>
	<groupId>uk.org.mygrid.provenance</groupId>
	<artifactId>logbook-ui</artifactId>
	<version>1.2.8</version>
	<name>Logbook user interface</name>
	<description>Provenance Browser for Taverna</description>
	<build>
		<resources>
			<resource>
				<directory>resources</directory>
			</resource>
		</resources>
		<sourceDirectory>src</sourceDirectory>
		<extensions>
			<extension>
				<groupId>org.apache.maven.wagon</groupId>
				<artifactId>wagon-ssh-external</artifactId>
				<version>1.0-alpha-5</version>
			</extension>
		</extensions>
		<plugins>
			<plugin>
				<artifactId>maven-surefire-plugin</artifactId>
				<configuration>
					<skip>true</skip>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<dependencies>
		<dependency>
			<groupId>uk.org.mygrid.provenance</groupId>
			<artifactId>logbook</artifactId>
			<version>1.2.8</version>
		</dependency>
		<dependency>
			<groupId>uk.org.mygrid.provenance</groupId>
			<artifactId>datalineage</artifactId>
			<version>1.0.4</version>
		</dependency>
		<!-- 
			<dependency>
			<groupId>uk.org.mygrid.provenance</groupId>
			<artifactId>logbook-boca</artifactId>
			<version>1.0.0-SNAPSHOT</version>
			</dependency>
		-->
		<dependency>
			<groupId>uk.org.mygrid.taverna</groupId>
			<artifactId>taverna-workbench</artifactId>
			<version>1.6.2.0</version>
		</dependency>
		<dependency>
			<groupId>
				uk.org.mygrid.taverna.scufl.scufl-ui-components
			</groupId>
			<artifactId>scufl-diagram-panel</artifactId>
			<version>1.6.2.0</version>
		</dependency>
		<dependency>
			<groupId>uk.org.mygrid.taverna.perspectives</groupId>
			<artifactId>perspective-design</artifactId>
			<version>1.6.2.0</version>
		</dependency>
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<version>4.0</version>
			<scope>test</scope>
		</dependency>
	</dependencies>
</project>
