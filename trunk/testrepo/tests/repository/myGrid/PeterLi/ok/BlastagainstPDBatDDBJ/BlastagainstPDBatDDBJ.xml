<?xml version="1.0" encoding="UTF-8"?>
<s:scufl xmlns:s="http://org.embl.ebi.escience/xscufl/0.1alpha" version="0.1" log="3">
  <s:processor name="emblid_default">
    <s:stringconstant>X07024</s:stringconstant>
  </s:processor>
  <s:processor name="getFASTA">
    <s:arbitrarywsdl>
      <s:wsdl>http://xml.nig.ac.jp/wsdl/GetEntry.wsdl</s:wsdl>
      <s:operation>getFASTA_DDBJEntry</s:operation>
    </s:arbitrarywsdl>
  </s:processor>
  <s:processor name="blast_program">
    <s:stringconstant>blastx</s:stringconstant>
  </s:processor>
  <s:processor name="blast_db">
    <s:stringconstant>PDB</s:stringconstant>
  </s:processor>
  <s:processor name="blastx_ddbj">
    <s:description>Blast service at XML Central of DDBJ http://xml.nig.ac.jp/wsdl/index.jsp
Documentation http://xml.nig.ac.jp/doc/Blast.txt</s:description>
    <s:arbitrarywsdl>
      <s:wsdl>http://xml.nig.ac.jp/wsdl/Blast.wsdl</s:wsdl>
      <s:operation>searchSimple</s:operation>
    </s:arbitrarywsdl>
  </s:processor>
  <s:link>
    <s:input>getFASTA:accession</s:input>
    <s:output>emblid_default:value</s:output>
  </s:link>
  <s:link>
    <s:input>fasta_output</s:input>
    <s:output>getFASTA:Result</s:output>
  </s:link>
  <s:link>
    <s:input>blastx_ddbj:program</s:input>
    <s:output>blast_program:value</s:output>
  </s:link>
  <s:link>
    <s:input>blastx_ddbj:database</s:input>
    <s:output>blast_db:value</s:output>
  </s:link>
  <s:link>
    <s:input>blast_result_ddbj</s:input>
    <s:output>blastx_ddbj:Result</s:output>
  </s:link>
  <s:link>
    <s:input>blastx_ddbj:query</s:input>
    <s:output>getFASTA:Result</s:output>
  </s:link>
  <s:sink>
    fasta_output
    <s:metadata>
      <s:mimeTypes />
      <s:description />
      <s:semanticType />
    </s:metadata>
  </s:sink>
  <s:sink>
    blast_result_ddbj
    <s:metadata>
      <s:mimeTypes>
        <s:mimeType>text/plain</s:mimeType>
      </s:mimeTypes>
      <s:description>BLAST report</s:description>
      <s:semanticType />
    </s:metadata>
  </s:sink>
</s:scufl>


