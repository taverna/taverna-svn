<pattern name="annotation.GenomeAnnotationFlatfile">
  <description>Review a single genome annotation flatfile (EMBL, GenBank, GFF etc.)</description>
  <input name="title" type="'text/plain'"/>
  <input name="annotation" type="'text/plain'"/>
  <input name="comment" type="'text/plain'"/>
  <output name="decision" type="'text/plain'"/>
  <output name="review_notes" type="'text/plain'"/>
  <output name="reviewed_result" type="'text/plain'"/>
  <output name="modified" type="'text/plain'"/>
</pattern>