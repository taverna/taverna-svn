<?xml version="1.0" encoding="UTF-8"?>
<s:scufl xmlns:s="http://org.embl.ebi.escience/xscufl/0.1alpha" version="0.2" log="0">
  <s:workflowdescription lsid="urn:lsid:www.mygrid.org.uk:operation:H86ZQCFRPS0" author="Daniele Turi" title="Nested Workflow Failure Example">Nested Workflow Failure Example</s:workflowdescription>
  <s:processor name="concat">
    <s:local>org.embl.ebi.escience.scuflworkers.java.StringConcat</s:local>
  </s:processor>
  <s:processor name="NestedWorkflow">
    <s:description>Failure Example for Nested Workflows</s:description>
    <s:workflow>
      <s:scufl version="0.2" log="0">
        <s:workflowdescription lsid="urn:lsid:www.mygrid.org.uk:operation:R2OY7ERQ5G0:1" author="Daniele Turi" title="Failure Example for Nested Workflows">Failure Example for Nested Workflows</s:workflowdescription>
        <s:processor name="String_Constant" boring="true">
          <s:stringconstant>tr</s:stringconstant>
        </s:processor>
        <s:processor name="String_Constant1" boring="true">
          <s:stringconstant>ue</s:stringconstant>
        </s:processor>
        <s:processor name="Fail_if_true">
          <s:local>org.embl.ebi.escience.scuflworkers.java.FailIfTrue</s:local>
        </s:processor>
        <s:processor name="Fail_if_true1">
          <s:local critical="true">org.embl.ebi.escience.scuflworkers.java.FailIfTrue</s:local>
        </s:processor>
        <s:processor name="Concatenate_two_strings">
          <s:local>org.embl.ebi.escience.scuflworkers.java.StringConcat</s:local>
        </s:processor>
        <s:link source="Concatenate_two_strings:output" sink="Fail_if_true1:test" />
        <s:link source="String_Constant1:value" sink="Concatenate_two_strings:string2" />
        <s:link source="String_Constant1:value" sink="out2" />
        <s:link source="String_Constant:value" sink="Concatenate_two_strings:string1" />
        <s:link source="String_Constant:value" sink="Fail_if_true:test" />
        <s:link source="String_Constant:value" sink="out" />
        <s:sink name="out" />
        <s:sink name="out2" />
      </s:scufl>
    </s:workflow>
  </s:processor>
  <s:link source="NestedWorkflow:out" sink="concat:string1" />
  <s:link source="NestedWorkflow:out2" sink="concat:string2" />
</s:scufl>

