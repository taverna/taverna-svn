<?xml version="1.0" encoding="UTF-8"?>
<s:scufl xmlns:s="http://org.embl.ebi.escience/xscufl/0.1alpha" version="0.1" log="3">
  <s:processor name="databaseid">
    <s:description>Database Id representing the database which is to be Blasted</s:description>
    <s:stringconstant>htgo_mus</s:stringconstant>
  </s:processor>
  <s:processor name="Blastn">
    <s:description>Performs a Blastn analysis</s:description>
    <s:soaplabwsdl>http://www.ebi.ac.uk/collab/mygrid/service4/soap/services/alignment::blastn_ncbi</s:soaplabwsdl>
  </s:processor>
  <s:processor name="AffyMapper_seq">
    <s:description>Retrieves the nucleotide sequence associated with the given Affymetrix probe set Id</s:description>
    <s:arbitrarywsdl>
      <s:wsdl>http://mygrid.ncl.ac.uk/axis/services/affymapper?wsdl</s:wsdl>
      <s:operation>getSequence</s:operation>
    </s:arbitrarywsdl>
  </s:processor>
  <s:processor name="probeSetId">
    <s:stringconstant>1001_at</s:stringconstant>
  </s:processor>
  <s:link>
    <s:input>results_AffyMapper_seq</s:input>
    <s:output>AffyMapper_seq:getSequenceReturn</s:output>
  </s:link>
  <s:link>
    <s:input>results_Blastn</s:input>
    <s:output>Blastn:search_result</s:output>
  </s:link>
  <s:link>
    <s:input>Blastn:database</s:input>
    <s:output>databaseid:value</s:output>
  </s:link>
  <s:link>
    <s:input>Blastn:query_sequence</s:input>
    <s:output>AffyMapper_seq:getSequenceReturn</s:output>
  </s:link>
  <s:link>
    <s:input>AffyMapper_seq:probeSetId</s:input>
    <s:output>probeSetId:value</s:output>
  </s:link>
  <s:sink>
    results_AffyMapper_seq
    <s:metadata>
      <s:mimeTypes />
      <s:description>Nucleotide sequence in FASTA format</s:description>
      <s:semanticType>http://www.mygrid.org.uk/ontology#nucleotide_sequence_data</s:semanticType>
    </s:metadata>
  </s:sink>
  <s:sink>
    results_Blastn
    <s:metadata>
      <s:mimeTypes />
      <s:description>Report from a Blast search</s:description>
      <s:semanticType>http://www.mygrid.org.uk/ontology#bioinformatics_report</s:semanticType>
    </s:metadata>
  </s:sink>
</s:scufl>


