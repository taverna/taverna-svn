<?xml version="1.0" encoding="UTF-8"?>
<s:scufl xmlns:s="http://org.embl.ebi.escience/xscufl/0.1alpha" version="0.1" log="0">
  <s:processor name="fontSize">
    <s:stringconstant>24.00</s:stringconstant>
  </s:processor>
  <s:processor name="textToBraille">
    <s:stringconstant>Foo, and also bar</s:stringconstant>
  </s:processor>
  <s:processor name="convertText">
    <s:arbitrarywsdl>
      <s:wsdl>http://www.webservicex.net/braille.asmx?WSDL</s:wsdl>
      <s:operation>BrailleText</s:operation>
    </s:arbitrarywsdl>
  </s:processor>
  <s:link>
    <s:input>convertText:InText</s:input>
    <s:output>textToBraille:value</s:output>
  </s:link>
  <s:link>
    <s:input>convertText:TextFontSize</s:input>
    <s:output>fontSize:value</s:output>
  </s:link>
  <s:link>
    <s:input>brailleImage</s:input>
    <s:output>convertText:BrailleTextResult</s:output>
  </s:link>
  <s:sink>
    brailleImage
    <s:metadata>
      <s:mimeTypes />
      <s:description />
      <s:semanticType />
    </s:metadata>
  </s:sink>
</s:scufl>


