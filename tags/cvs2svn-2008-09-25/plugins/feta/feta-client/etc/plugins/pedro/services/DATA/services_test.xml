		<organisation>
			<organisationName>European Bioinformatics Institute</organisationName>
			<organisationDescriptionText>EBI</organisationDescriptionText>
		</organisation>
		<locationURL>http://www.ebi.ac.uk/collab/mygrid/service4/soap/services/alignment::blastx_ncbi</locationURL>
		<interfaceWSDL>http://www.ebi.ac.uk/collab/mygrid/service4/soap/services/alignment::blastx_ncbi?wsdl</interfaceWSDL>
		<serviceDescriptionText>Converts a nucleotide query sequence into protein sequences in all 6 reading frames. The translated protein products are then compared against the  NCBI protein databases</serviceDescriptionText>
		<serviceType>Soaplab service</serviceType>				
		<operations>
			<serviceOperation>
				<operationName>blastx_ncbi</operationName>
				<operationInputs>
					<parameter>
						<parameterName>query_sequence</parameterName>
						<semanticType>nucleotide_sequence</semanticType>
						<formats>
							<formatIdentifier>FASTA format</formatIdentifier>
						</formats>
					</parameter>
					<parameter>
						<parameterName>database</parameterName>
						<semanticType>bioinformatics_record_id</semanticType>
					</parameter>
				</operationInputs>
				<operationOutputs>
					<parameter>
						<parameterName>search_result</parameterName>
						<parameterDescription>search_result</parameterDescription>
						<semanticType>biological_sequence_alignment_data</semanticType>
						<formats>
							<formatIdentifier>Blastx_search_result_format</formatIdentifier>
						</formats>
					</parameter>
				</operationOutputs>				
				<operationTask>local_aligning</operationTask>
				<operationResource>sequence_database</operationResource>
				<operationApplication>Basic_Local_Alignment_Search_Tool</operationApplication>				
			</serviceOperation>
		</operations>
	</serviceDescription>	
	<serviceDescription>
