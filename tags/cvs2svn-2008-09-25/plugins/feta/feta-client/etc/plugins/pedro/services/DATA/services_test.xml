	<serviceDescription>
		<serviceName>EmblSnpEdit</serviceName>
		<organisation>
			<organisationName>Newcastle University</organisationName>
			<organisationDescriptionText>Bioinformatics group</organisationDescriptionText>
		</organisation>
		<locationURL>http://mygrid.ncl.ac.uk/axis/services/EmblSnpEdit</locationURL>
		<interfaceWSDL>http://mygrid.ncl.ac.uk/axis/services/EmblSnpEdit?wsdl</interfaceWSDL>
		<serviceType>WSDL service</serviceType>		
		<operations>
			<serviceOperation>
				<operationName>mergeSnpData</operationName>
				<operationInputs>
					<parameter>
						<parameterName>hgvbaseEntries</parameterName>
						<isConfigurationParameter>false</isConfigurationParameter>
