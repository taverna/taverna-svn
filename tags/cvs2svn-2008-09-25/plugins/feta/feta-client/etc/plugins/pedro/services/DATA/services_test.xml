							<formatIdentifier>EMBOSS_restriction_enzyme_report_format</formatIdentifier>
						</formats>
					</parameter>				
				</operationOutputs>				
				<operationTask>calculating</operationTask>
				<operationResource>REBASE_database</operationResource>
			</serviceOperation>
		</operations>
	</serviceDescription>	
	<serviceDescription>
		<serviceName>nucleic_primers__eprimer3__derived</serviceName>
		<organisation>
			<organisationName>European Bioinformatics Institute</organisationName>
			<organisationDescriptionText>EBI</organisationDescriptionText>
		</organisation>
		<locationURL>http://industry.ebi.ac.uk/soap/soaplab/nucleic_primers::eprimer3</locationURL>
		<interfaceWSDL>http://industry.ebi.ac.uk/soap/soaplab/nucleic_primers::eprimer3</interfaceWSDL>
		<serviceDescriptionText>Picks PCR primers and hybridization oligos</serviceDescriptionText>
		<serviceType>Soaplab service</serviceType>		
