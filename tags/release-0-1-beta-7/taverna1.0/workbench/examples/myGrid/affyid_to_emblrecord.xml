<?xml version="1.0" encoding="UTF-8"?>
<s:scufl xmlns:s="http://org.embl.ebi.escience/xscufl/0.1alpha" version="0.1" log="3">
  <s:processor name="srsebi_getemblrecord">
    <s:description>Retrieves the EMBL record associated with the EMBL accession number using the SRS web service</s:description>
    <s:arbitrarywsdl>
      <s:wsdl>http://mygrid.ncl.ac.uk/axis/services/SrsEbiQuery?wsdl</s:wsdl>
      <s:operation>queryById</s:operation>
    </s:arbitrarywsdl>
  </s:processor>
  <s:processor name="affymapper_getemblaccnumber">
    <s:description>Retrieves the mapping between Affymetrix probe set Id and its EMBL accession number</s:description>
    <s:arbitrarywsdl>
      <s:wsdl>http://mygrid.ncl.ac.uk/axis/services/affymapper?wsdl</s:wsdl>
      <s:operation>getAccessionNumber</s:operation>
    </s:arbitrarywsdl>
  </s:processor>
  <s:processor name="probeSetId">
    <s:stringconstant>134_at</s:stringconstant>
  </s:processor>
  <s:link>
    <s:input>srsebi_getemblrecord:usa</s:input>
    <s:output>affymapper_getemblaccnumber:getAccessionNumberReturn</s:output>
  </s:link>
  <s:link>
    <s:input>emblrecord</s:input>
    <s:output>srsebi_getemblrecord:queryByIdReturn</s:output>
  </s:link>
  <s:link>
    <s:input>affymapper_getemblaccnumber:probeSetId</s:input>
    <s:output>probeSetId:value</s:output>
  </s:link>
  <s:sink>
    emblrecord
    <s:metadata>
      <s:mimeTypes>
        <s:mimeType>text/plain</s:mimeType>
      </s:mimeTypes>
      <s:description />
      <s:semanticType>http://www.mygrid.org.uk/ontology#EMBL_record</s:semanticType>
    </s:metadata>
  </s:sink>
</s:scufl>


