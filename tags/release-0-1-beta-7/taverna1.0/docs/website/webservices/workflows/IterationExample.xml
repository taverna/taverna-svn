<?xml version="1.0" encoding="UTF-8"?>
<s:scufl xmlns:s="http://org.embl.ebi.escience/xscufl/0.1alpha" version="0.1" log="0">
  <s:processor name="OntologyRootID">
    <s:stringconstant>GO:0003673</s:stringconstant>
  </s:processor>
  <s:processor name="getChildren">
    <s:arbitrarywsdl>
      <s:wsdl>http://mygrid.ncl.ac.uk/axis/services/GoQuery?wsdl</s:wsdl>
      <s:operation>getChildrenIds</s:operation>
    </s:arbitrarywsdl>
  </s:processor>
  <s:processor name="getGrandChildren">
    <s:arbitrarywsdl>
      <s:wsdl>http://mygrid.ncl.ac.uk/axis/services/GoQuery?wsdl</s:wsdl>
      <s:operation>getChildrenIds</s:operation>
    </s:arbitrarywsdl>
  </s:processor>
  <s:processor name="getGrandChildNames">
    <s:arbitrarywsdl>
      <s:wsdl>http://mygrid.ncl.ac.uk/axis/services/GoQuery?wsdl</s:wsdl>
      <s:operation>getTerm</s:operation>
    </s:arbitrarywsdl>
  </s:processor>
  <s:processor name="getChildNames">
    <s:arbitrarywsdl>
      <s:wsdl>http://mygrid.ncl.ac.uk/axis/services/GoQuery?wsdl</s:wsdl>
      <s:operation>getTerm</s:operation>
    </s:arbitrarywsdl>
  </s:processor>
  <s:link>
    <s:input>getChildren:goId</s:input>
    <s:output>OntologyRootID:value</s:output>
  </s:link>
  <s:link>
    <s:input>getGrandChildren:goId</s:input>
    <s:output>getChildren:getChildrenIdsReturn</s:output>
  </s:link>
  <s:link>
    <s:input>getGrandChildNames:goId</s:input>
    <s:output>getGrandChildren:getChildrenIdsReturn</s:output>
  </s:link>
  <s:link>
    <s:input>grandchildren</s:input>
    <s:output>getGrandChildNames:getTermReturn</s:output>
  </s:link>
  <s:link>
    <s:input>children</s:input>
    <s:output>getChildNames:getTermReturn</s:output>
  </s:link>
  <s:link>
    <s:input>getChildNames:goId</s:input>
    <s:output>getChildren:getChildrenIdsReturn</s:output>
  </s:link>
  <s:sink>
    children
    <s:metadata>
      <s:mimeTypes />
      <s:description />
      <s:semanticType />
    </s:metadata>
  </s:sink>
  <s:sink>
    grandchildren
    <s:metadata>
      <s:mimeTypes />
      <s:description />
      <s:semanticType />
    </s:metadata>
  </s:sink>
</s:scufl>


