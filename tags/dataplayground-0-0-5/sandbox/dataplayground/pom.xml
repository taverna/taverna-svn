<?xml version="1.0" encoding="UTF-8"?>
<project>
	<modelVersion>4.0.0</modelVersion>
	<groupId>uk.org.mygrid.dataplayground</groupId>
	<artifactId>dataplayground</artifactId>
	<version>0.0.5</version>
	<name>Data playground</name>
	<description>Data playground</description>
	<issueManagement>
		<system>jira</system>
		<url>http://www.mygrid.org.uk/jira/</url>
	</issueManagement>
	<ciManagement>
		<system>continuum</system>
		<url>http://www.mygrid.org.uk/continuum/</url>
		<notifiers>
			<notifier>
				<type>mail</type>
				<sendOnError>false</sendOnError>
				<sendOnFailure>true</sendOnFailure>
				<sendOnSuccess>false</sendOnSuccess>
				<sendOnWarning>false</sendOnWarning>
				<configuration>
					<address>david.withers@manchester.ac.uk</address>
				</configuration>
			</notifier>
		</notifiers>
	</ciManagement>
	<mailingLists>
		<mailingList>
			<name>Taverna hackers</name>
			<subscribe>
				https://lists.sourceforge.net/lists/listinfo/taverna-hackers
			</subscribe>
			<post>taverna-hackers@lists.sourceforge.net</post>
			<archive>
				http://sourceforge.net/mailarchive/forum.php?forum=taverna-hackers
			</archive>
		</mailingList>
		<mailingList>
			<name>Taverna users</name>
			<subscribe>
				https://lists.sourceforge.net/lists/listinfo/taverna-users
			</subscribe>
			<post>taverna-users@lists.sourceforge.net</post>
			<archive>
				http://sourceforge.net/mailarchive/forum.php?forum=taverna-users
			</archive>
		</mailingList>
	</mailingLists>
	<licenses>
		<license>
			<name>GNU Lesser General Public License</name>
			<url>http://www.gnu.org/licenses/lgpl.txt</url>
			<distribution>repo</distribution>
		</license>
	</licenses>
	<scm>
		<connection>
			scm:cvs:pserver:anonymous@taverna.cvs.sourceforge.net:/cvsroot/taverna:sandbox/dataplayground
		</connection>
		<url>
			http://taverna.cvs.sourceforge.net/taverna/sandbox/dataplayground/
		</url>
	</scm>
	<organization>
		<name>University of Manchester</name>
		<url>http://www.manchester.ac.uk/</url>
	</organization>
	<build>
		<plugins>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.5</source>
					<target>1.5</target>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-source-plugin</artifactId>
				<executions>
					<execution>
						<goals>
							<goal>jar</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>
	<profiles>
		<profile>
			<id>default-tools.jar</id>
			<activation>
				<property>
					<name>java.vendor</name>
					<value>Sun Microsystems Inc.</value>
				</property>
			</activation>
			<dependencies>
				<dependency>
					<groupId>com.sun</groupId>
					<artifactId>tools</artifactId>
					<version>1.4.2</version>
					<scope>system</scope>
					<systemPath>
						${java.home}/../lib/tools.jar
					</systemPath>
				</dependency>
			</dependencies>
		</profile>
	</profiles>
	<repositories>
		<repository>
			<releases>
				<enabled>false</enabled>
			</releases>
			<snapshots />
			<id>mygrid-snapshot-repository</id>
			<name>myGrid Snapshot Repository</name>
			<url>
				http://www.mygrid.org.uk/maven/snapshot-repository
			</url>
		</repository>
		<repository>
			<releases />
			<snapshots>
				<enabled>false</enabled>
			</snapshots>
			<id>mygrid-repository</id>
			<name>myGrid Repository</name>
			<url>http://www.mygrid.org.uk/maven/repository</url>
		</repository>
		<repository>
			<releases />
			<snapshots>
				<enabled>false</enabled>
			</snapshots>
			<id>mobycentral</id>
			<name>MobyCentral Repository</name>
			<url>http://mobycentral.icapture.ubc.ca/maven</url>
		</repository>
	</repositories>
	<pluginRepositories>
		<pluginRepository>
			<releases>
				<enabled>false</enabled>
			</releases>
			<snapshots />
			<id>Maven Snapshots</id>
			<url>http://snapshots.maven.codehaus.org/maven2/</url>
		</pluginRepository>
		<pluginRepository>
			<releases>
				<enabled>false</enabled>
			</releases>
			<snapshots />
			<id>mygrid-snapshot-repository</id>
			<name>myGrid Snapshot Repository</name>
			<url>
				http://www.mygrid.org.uk/maven/snapshot-repository
			</url>
		</pluginRepository>
		<pluginRepository>
			<releases />
			<snapshots>
				<enabled>false</enabled>
			</snapshots>
			<id>mygrid-repository</id>
			<name>myGrid Repository</name>
			<url>http://www.mygrid.org.uk/maven/repository</url>
		</pluginRepository>
	</pluginRepositories>
	<distributionManagement>
		<repository>
			<id>mygrid-repository</id>
			<name>myGrid Repository</name>
			<url>
				scpexe://rosalind.cs.man.ac.uk/local/www/mygrid/maven/repository
			</url>
		</repository>
		<snapshotRepository>
			<id>mygrid-snapshot-repository</id>
			<name>myGrid Snapshot Repository</name>
			<url>
				scpexe://rosalind.cs.man.ac.uk/local/www/mygrid/maven/snapshot-repository
			</url>
		</snapshotRepository>
		<site>
			<id>test-site</id>
			<name>test-site</name>
			<url>file:///</url>
		</site>
	</distributionManagement>
	<dependencies>
		<dependency>
			<groupId>uk.org.mygrid.taverna.scufl</groupId>
			<artifactId>scufl-tools</artifactId>
			<version>1.7.0.0</version>
		</dependency>
		<dependency>
			<groupId>uk.org.mygrid.taverna.scufl</groupId>
			<artifactId>scufl-ui-api</artifactId>
			<version>1.7.0.0</version>
		</dependency>
		<dependency>
			<groupId>uk.org.mygrid.taverna.perspectives</groupId>
			<artifactId>perspective-core</artifactId>
			<version>1.7.0.0</version>
		</dependency>
		<dependency>
			<groupId>uk.org.mygrid.taverna</groupId>
			<artifactId>taverna-scavenger-ui</artifactId>
			<version>1.7.0.0</version>
		</dependency>
		<dependency>
			<groupId>biomoby.org</groupId>
			<artifactId>taverna-biomoby</artifactId>
			<version>1.7.0.0</version>
		</dependency>
		<dependency>
			<groupId>colt</groupId>
			<artifactId>colt</artifactId>
			<version>1.2.0</version>
		</dependency>
		<dependency>
			<groupId>concurrent</groupId>
			<artifactId>concurrent</artifactId>
			<version>1.3.4</version>
		</dependency>
		<dependency>
			<groupId>jung</groupId>
			<artifactId>jung</artifactId>
			<version>1.7.6</version>
		</dependency>
		<dependency>
			<groupId>commons-collections</groupId>
			<artifactId>commons-collections</artifactId>
			<version>3.1</version>
		</dependency>
	</dependencies>
</project>