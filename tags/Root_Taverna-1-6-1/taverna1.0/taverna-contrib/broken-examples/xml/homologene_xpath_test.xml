<?xml version="1.0" encoding="UTF-8"?>
<s:scufl xmlns:s="http://org.embl.ebi.escience/xscufl/0.1alpha" version="0.2" log="0">
  <s:workflowdescription lsid="urn:lsid:www.mygrid.org.uk:operation:Z7HX2Y1OUI0" author="" title="HomoloGene XPath test" />
  <s:processor name="maxrecs">
    <s:stringconstant>1</s:stringconstant>
  </s:processor>
  <s:processor name="HomoloGene_XML">
    <s:local>net.sourceforge.taverna.scuflworkers.ncbi.HomoloGeneWorker</s:local>
  </s:processor>
  <s:processor name="term">
    <s:stringconstant>brca2</s:stringconstant>
  </s:processor>
  <s:processor name="XPath">
    <s:local>net.sourceforge.taverna.scuflworkers.xml.XPathWorker</s:local>
  </s:processor>
  <s:processor name="xpath_str">
    <s:stringconstant>edit me!</s:stringconstant>
  </s:processor>
  <s:link source="maxrecs:value" sink="HomoloGene_XML:maxRecords" />
  <s:link source="term:value" sink="HomoloGene_XML:term" />
  <s:link source="HomoloGene_XML:resultsXml" sink="xml" />
  <s:sink name="xml">
    <s:metadata>
      <s:mimeTypes>
        <s:mimeType>text/xml</s:mimeType>
      </s:mimeTypes>
    </s:metadata>
  </s:sink>
</s:scufl>


