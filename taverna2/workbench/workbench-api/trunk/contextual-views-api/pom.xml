<?xml version="1.0" encoding="UTF-8"?>
<project>
	<modelVersion>4.0.0</modelVersion>
	<groupId>net.sf.taverna.t2.workbench</groupId>
	<artifactId>contextual-views-api</artifactId>
	<name>Contextual Views API</name>
	<parent>
		<groupId>net.sf.taverna.t2</groupId>
		<artifactId>t2-workbench</artifactId>
		<version>0.3-SNAPSHOT</version>
	</parent>
	<description>Contextual views for the activities</description>
	<build>
		<plugins>
			<plugin>
				<artifactId>maven-surefire-plugin</artifactId>
				<configuration>
					<systemProperties>
						<property>
							<name>java.awt.headless</name>
							<value>false</value>
						</property>
					</systemProperties>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<dependencies>
	
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>net.sf.taverna.t2</groupId>
			<artifactId>spi-discovery-api</artifactId>
			<version>${t2.spidiscovery.version}</version>
		</dependency>
		<dependency>
			<groupId>net.sf.taverna.t2.workbench</groupId>
			<artifactId>configuration-impl</artifactId>
			<version>${t2.workbench.version}</version>
		</dependency>
		<dependency>
			<groupId>net.sf.taverna.t2</groupId>
			<artifactId>workflowmodel-api</artifactId>
			<version>${t2.core.version}</version>
		</dependency>
		<dependency>
			<groupId>net.sf.taverna.t2.workbench</groupId>
			<artifactId>ui-api</artifactId>
			<version>${t2.workbench.version}</version>
		</dependency>
		<dependency>
			<groupId>net.sf.taverna.t2.lang</groupId>
			<artifactId>ui</artifactId>
			<version>${t2.ui.version}</version>
		</dependency>
		<dependency>
			<groupId>net.sf.taverna.t2.workbench</groupId>
			<artifactId>ui-impl</artifactId>
			<version>${t2.workbench.version}</version>
		</dependency>
		<dependency>
			<groupId>net.sf.taverna.t2.workbench</groupId>
			<artifactId>file-api</artifactId>
			<version>${t2.workbench.version}</version>
		</dependency>

	</dependencies>
</project>
