<?xml version="1.0" encoding="UTF-8"?>
<project>
    <parent>
        <artifactId>instantsoap</artifactId>
        <groupId>uk.ac.ncl.cs.instantsoap</groupId>
        <version>1.0-SNAPSHOT</version>
    </parent>
    <modelVersion>4.0.0</modelVersion>
    <groupId>uk.ac.ncl.cs.instantsoap</groupId>
    <artifactId>taverna-instantsoap-processor</artifactId>
    <name>InstantSoap Taverna Processor</name>
    <version>1.0-SNAPSHOT</version>

    <dependencies>
        <dependency>
            <groupId>uk.org.mygrid.taverna</groupId>
            <artifactId>taverna-scavenger</artifactId>
            <version>1.7.1.0</version>
        </dependency>
        <dependency>
            <groupId>uk.org.mygrid.taverna.baclava</groupId>
            <artifactId>baclava-core</artifactId>
            <version>1.7.1.0</version>
        </dependency>
        <dependency>
            <groupId>uk.org.mygrid.taverna</groupId>
            <artifactId>taverna-core</artifactId>
            <version>1.7.1.0</version>
        </dependency>
        <dependency>
            <groupId>uk.org.mygrid.taverna.scufl</groupId>
            <artifactId>scufl-model</artifactId>
            <version>1.7.1.0</version>
        </dependency>
        <dependency>
            <groupId>uk.org.mygrid.taverna.scufl</groupId>
            <artifactId>scufl-ui-api</artifactId>
            <version>1.7.1.0</version>
        </dependency>
        <!--<dependency>
            <groupId>uk.org.mygrid.resources</groupId>
            <artifactId>axis</artifactId>
            <version>1.4-taverna</version>
        </dependency>-->

        <dependency>
            <groupId>log4j</groupId>
            <artifactId>log4j</artifactId>
            <version>1.2.12</version>
        </dependency>

        <!-- Instantsoap -->
        <dependency>
            <groupId>uk.ac.ncl.cs.instantsoap</groupId>
            <artifactId>instantsoap-api</artifactId>
            <version>1.0-SNAPSHOT</version>
        </dependency>

        <!-- CXF -->
        <dependency>
            <groupId>org.apache.cxf</groupId>
            <artifactId>cxf-rt-frontend-jaxws</artifactId>
            <version>2.1-SNAPSHOT</version>
            <exclusions>
                <exclusion>
                    <groupId>javax.jws</groupId>
                    <artifactId>jsr181-api</artifactId>
                </exclusion>
            </exclusions>
        </dependency>
        <dependency>
            <groupId>org.apache.cxf</groupId>
            <artifactId>cxf-rt-transports-http</artifactId>
            <version>2.1-SNAPSHOT</version>
            <exclusions>
                <exclusion>
                    <groupId>javax.jws</groupId>
                    <artifactId>jsr181-api</artifactId>
                </exclusion>
            </exclusions>
        </dependency>

    </dependencies>

    <repositories>
        <repository>
            <id>mygrid-repository</id>
            <name>myGrid Repository</name>
            <url>http://www.mygrid.org.uk/maven/repository</url>
            <snapshots>
                <enabled>false</enabled>
            </snapshots>
            <releases>
                <enabled>true</enabled>
            </releases>
        </repository>
        <repository>
            <id>mygrid-snapshot-repository</id>
            <name>myGrid Snapshot Repository</name>
            <url>http://www.mygrid.org.uk/maven/snapshot-repository</url>
            <snapshots>
                <enabled>true</enabled>
            </snapshots>
            <releases>
                <enabled>false</enabled>
            </releases>
        </repository>

        <repository>
            <id>apache-snapshots</id>
            <name>Apache SNAPSHOT Repository</name>
            <url>http://people.apache.org/repo/m2-snapshot-repository/</url>
            <snapshots>
                <enabled>true</enabled>
            </snapshots>
        </repository>
        <repository>
            <id>apache-incubating</id>
            <name>Apache Incubating Repository</name>
            <url>http://people.apache.org/repo/m2-incubating-repository/</url>
        </repository>

        <!-- for jaxb-impl -->
        <repository>
            <id>java.net</id>
            <url>http://download.java.net/maven/1/</url>
            <layout>legacy</layout>
        </repository>
    </repositories>
</project>
