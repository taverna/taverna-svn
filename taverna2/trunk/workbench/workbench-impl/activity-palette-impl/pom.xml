<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>net.sf.taverna.t2.workbench</groupId>
	<artifactId>activity-palette-impl</artifactId>
	<name>Activity Palette Impl</name>
	<parent>
		<groupId>net.sf.taverna.t2</groupId>
		<artifactId>t2-workbench</artifactId>
		<version>0.3-SNAPSHOT</version>
	</parent>
	<description>Activity Palette Implementation</description>
	<build>
		<plugins>
			<plugin>
				<artifactId>maven-surefire-plugin</artifactId>
				<configuration>
					<systemProperties>
						<property>
							<name>java.awt.headless</name>
							<value>false</value>
						</property>
					</systemProperties>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<dependencies>
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<scope>test</scope>
		</dependency>
		
		<dependency>
			<groupId>net.sf.taverna.t2.workbench</groupId>
			<artifactId>activity-palette-api</artifactId>
			<version>${t2.workbench.version}</version>
		</dependency>
		
		<dependency>
			<groupId>net.sf.taverna.t2</groupId>
			<artifactId>partition</artifactId>
			<version>${t2.partition.version}</version>
		</dependency>
		<dependency>
			<groupId>commons-collections</groupId>
			<artifactId>commons-collections</artifactId>
		</dependency>
	</dependencies>
</project>
