<?xml version="1.0" encoding="UTF-8"?>
<project>
  <parent>
    <artifactId>taverna</artifactId>
    <groupId>uk.org.mygrid.taverna</groupId>
    <version>1.5-SNAPSHOT</version>
  </parent>
  <modelVersion>4.0.0</modelVersion>
  <groupId>uk.org.mygrid.taverna</groupId>
  <artifactId>taverna-workbench</artifactId>
  <name>Taverna Workbench</name>
  <version>1.5-SNAPSHOT</version>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-antrun-plugin</artifactId>
        <executions>
          <execution>
            <id>jreleaseinfo</id>
            <phase>generate-sources</phase>
            <goals>
              <goal>run</goal>
            </goals>
            <configuration>
              <tasks>
                <taskdef classname="ch.oscg.jreleaseinfo.anttask.JReleaseInfoAntTask" name="jreleaseinfo">
                  <classpath refid="maven.compile.classpath"/>
                </taskdef>
                <jreleaseinfo packageName="org.embl.ebi.escience.scufl" targetDir="${project.build.directory}/generated-sources/jreleaseinfo" project="${artifactId}" className="TavernaReleaseInfo" withViewer="false" version="${version}"></jreleaseinfo>
              </tasks>
              <sourceRoot>${project.build.directory}/generated-sources/jreleaseinfo</sourceRoot>
            </configuration>
          </execution>
        </executions>
        <dependencies>
          <dependency>
            <groupId>uk.org.mygrid.resources</groupId>
            <artifactId>jreleaseinfo</artifactId>
            <version>1.2.0</version>
          </dependency>
        </dependencies>
      </plugin>
    </plugins>
  </build>
  <dependencies>
    <dependency>
      <groupId>uk.org.mygrid.taverna</groupId>
      <artifactId>taverna-core</artifactId>
    </dependency>
    <dependency>
      <groupId>uk.org.mygrid.taverna</groupId>
      <artifactId>taverna-enactor</artifactId>
    </dependency>
    <dependency>
      <groupId>uk.org.mygrid.taverna</groupId>
      <artifactId>taverna-scavenger-ui</artifactId>
    </dependency>
    <dependency>
      <groupId>uk.org.mygrid.taverna.scufl</groupId>
      <artifactId>scufl-model</artifactId>
    </dependency>
    <dependency>
      <groupId>uk.org.mygrid.taverna.scufl</groupId>
      <artifactId>scufl-ui</artifactId>
    </dependency>
  </dependencies>
</project>
