<?xml version="1.0" encoding="UTF-8"?>
<s:scufl xmlns:s="http://org.embl.ebi.escience/xscufl/0.1alpha" version="0.2" log="0">
  <s:workflowdescription lsid="urn:lsid:www.mygrid.org.uk:operation:EAIF0TPRK30" author="" title="" />
  <s:processor name="content" boring="true">
    <s:stringconstant>content_one,content_two,content_three,content_four</s:stringconstant>
  </s:processor>
  <s:processor name="contentXML">
    <s:local>
      org.embl.ebi.escience.scuflworkers.java.XMLInputSplitter
      <s:extensions>
        <s:arraytype optional="false" unbounded="false" typename="WSArrayofData" name="content">
          <s:elementtype>
            <s:complextype optional="false" unbounded="false" typename="data" name="">
              <s:elements>
                <s:basetype optional="false" unbounded="false" typename="string" name="type" />
                <s:basetype optional="false" unbounded="false" typename="string" name="content" />
              </s:elements>
            </s:complextype>
          </s:elementtype>
        </s:arraytype>
      </s:extensions>
    </s:local>
  </s:processor>
  <s:processor name="type" boring="true">
    <s:stringconstant>type_one,type_two,type_three,type_four</s:stringconstant>
  </s:processor>
  <s:processor name="WSArrayofDataXML">
    <s:local>
      org.embl.ebi.escience.scuflworkers.java.XMLInputSplitter
      <s:extensions>
        <s:complextype optional="false" unbounded="false" typename="data" name="">
          <s:elements>
            <s:basetype optional="false" unbounded="false" typename="string" name="type" />
            <s:basetype optional="false" unbounded="false" typename="string" name="content" />
          </s:elements>
        </s:complextype>
      </s:extensions>
    </s:local>
    <s:iterationstrategy>
      <i:dot xmlns:i="http://org.embl.ebi.escience/xscufliteration/0.1beta10">
        <i:iterator name="type" />
        <i:iterator name="content" />
      </i:dot>
    </s:iterationstrategy>
  </s:processor>
  <s:processor name="split_type">
    <s:local>org.embl.ebi.escience.scuflworkers.java.SplitByRegex</s:local>
  </s:processor>
  <s:processor name="split_content">
    <s:local>org.embl.ebi.escience.scuflworkers.java.SplitByRegex</s:local>
  </s:processor>
  <s:link source="WSArrayofDataXML:output" sink="contentXML:WSArrayofData" />
  <s:link source="content:value" sink="split_content:string" />
  <s:link source="split_content:split" sink="WSArrayofDataXML:content" />
  <s:link source="split_type:split" sink="WSArrayofDataXML:type" />
  <s:link source="type:value" sink="split_type:string" />
  <s:link source="contentXML:output" sink="out" />
  <s:sink name="out" />
</s:scufl>


