<?xml version="1.0" encoding="UTF-8"?>
<s:scufl xmlns:s="http://org.embl.ebi.escience/xscufl/0.1alpha" version="0.1" log="0">
  <s:processor name="os_format">
    <s:stringconstant>fasta</s:stringconstant>
  </s:processor>
  <s:processor name="sformat">
    <s:stringconstant>embl</s:stringconstant>
  </s:processor>
  <s:processor name="organism">
    <s:stringconstant>arabidopsis</s:stringconstant>
  </s:processor>
  <s:processor name="name">
    <s:stringconstant>sequence</s:stringconstant>
  </s:processor>
  <s:processor name="srs_getEmblRecord">
    <s:arbitrarywsdl>
      <s:wsdl>http://mygrid.ncl.ac.uk/axis/services/SrsEbiQuery?wsdl</s:wsdl>
      <s:operation>queryById</s:operation>
    </s:arbitrarywsdl>
  </s:processor>
  <s:processor name="seqret_toFasta">
    <s:description>Reads and writes (returns) sequences</s:description>
    <s:soaplabwsdl>http://industry.ebi.ac.uk/soap/soaplab/edit::seqret</s:soaplabwsdl>
  </s:processor>
  <s:processor name="glimmerm">
    <s:arbitrarywsdl>
      <s:wsdl>http://www.vbi.vt.edu/~pathport/services/wsdls/glimmerm.wsdl</s:wsdl>
      <s:operation>getGenepredict</s:operation>
    </s:arbitrarywsdl>
  </s:processor>
  <s:processor name="emblAccNumber">
    <s:stringconstant>embl-AccNumber:AC009325</s:stringconstant>
  </s:processor>
  <s:link>
    <s:input>seqret_toFasta:sequence_direct_data</s:input>
    <s:output>srs_getEmblRecord:queryByIdReturn</s:output>
  </s:link>
  <s:link>
    <s:input>seqret_toFasta:osformat</s:input>
    <s:output>os_format:value</s:output>
  </s:link>
  <s:link>
    <s:input>seqret_toFasta:sformat</s:input>
    <s:output>sformat:value</s:output>
  </s:link>
  <s:link>
    <s:input>glimmerm:in1</s:input>
    <s:output>seqret_toFasta:outseq</s:output>
  </s:link>
  <s:link>
    <s:input>glimmerm:in0</s:input>
    <s:output>name:value</s:output>
  </s:link>
  <s:link>
    <s:input>glimmerm:in2</s:input>
    <s:output>organism:value</s:output>
  </s:link>
  <s:link>
    <s:input>predicted_genes</s:input>
    <s:output>glimmerm:getGenepredictReturn</s:output>
  </s:link>
  <s:link>
    <s:input>srs_getEmblRecord:usa</s:input>
    <s:output>emblAccNumber:value</s:output>
  </s:link>
  <s:sink>
    predicted_genes
    <s:metadata>
      <s:mimeTypes />
      <s:description>Output from GlimmerM</s:description>
      <s:semanticType>http://www.mygrid.org.uk/ontology#bioinformatics_data_structure</s:semanticType>
    </s:metadata>
  </s:sink>
</s:scufl>


