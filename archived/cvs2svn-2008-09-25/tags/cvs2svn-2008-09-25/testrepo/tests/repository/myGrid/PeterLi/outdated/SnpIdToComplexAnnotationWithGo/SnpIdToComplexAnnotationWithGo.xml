      <s:wsdl>http://mygrid.ncl.ac.uk/axis/services/SrsEbiQuery?wsdl</s:wsdl>
      <s:operation>queryByXRef</s:operation>
    </s:arbitrarywsdl>
  </s:processor>
  <s:processor name="getSwissprotIds">
    <s:arbitrarywsdl>
      <s:wsdl>http://mygrid.ncl.ac.uk/axis/services/SrsEbiQuery?wsdl</s:wsdl>
      <s:operation>queryByXRef</s:operation>
    </s:arbitrarywsdl>
  </s:processor>
  <s:processor name="getEmblIds">
    <s:arbitrarywsdl>
      <s:wsdl>http://mygrid.ncl.ac.uk/axis/services/SrsEbiQuery?wsdl</s:wsdl>
      <s:operation>queryByXRef</s:operation>
    </s:arbitrarywsdl>
  </s:processor>
  <s:processor name="databaseid3">
    <s:stringconstant>EMBL</s:stringconstant>
  </s:processor>
