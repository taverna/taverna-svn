<?xml version="1.0" encoding="UTF-8"?>
<s:scufl xmlns:s="http://org.embl.ebi.escience/xscufl/0.1alpha" version="0.1" log="0">
  <s:processor name="GetEmblRecord">
    <s:arbitrarywsdl maxretries="0" retrydelay="0" retrybackoff="1.0">
      <s:wsdl>http://mygrid.ncl.ac.uk/axis/services/SrsEbiQuery?wsdl</s:wsdl>
      <s:operation>queryById</s:operation>
    </s:arbitrarywsdl>
  </s:processor>
  <s:processor name="showseq">
    <s:description>Displays information on the currently available databases</s:description>
    <s:soaplabwsdl maxretries="0" retrydelay="0" retrybackoff="1.0">http://industry.ebi.ac.uk/soap/soaplab/display::showdb</s:soaplabwsdl>
  </s:processor>
  <s:processor name="EmblAccNumber">
    <s:stringconstant maxretries="0" retrydelay="0" retrybackoff="1.0">embl-AccNumber:L00727</s:stringconstant>
  </s:processor>
  <s:link>
    <s:input>EmblRecord</s:input>
    <s:output>GetEmblRecord:queryByIdReturn</s:output>
  </s:link>
  <s:sink>
    EmblRecord
    <s:metadata>
      <s:mimeTypes />
      <s:description />
      <s:semanticType />
    </s:metadata>
  </s:sink>
</s:scufl>


