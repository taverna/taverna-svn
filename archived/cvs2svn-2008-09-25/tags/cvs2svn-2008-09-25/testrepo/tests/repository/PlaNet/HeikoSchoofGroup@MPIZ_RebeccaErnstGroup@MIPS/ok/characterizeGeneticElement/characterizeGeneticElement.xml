<?xml version="1.0" encoding="UTF-8"?>
<s:scufl xmlns:s="http://org.embl.ebi.escience/xscufl/0.1alpha" version="0.2" log="0">
  <s:workflowdescription lsid="urn:lsid:www.mygrid.org.uk:operation:QSRKRIXUG911" author="" title="" />
  <s:processor name="NS_Contig" boring="true">
    <s:stringconstant>MIPS_Contig_Medicago</s:stringconstant>
  </s:processor>
  <s:processor name="ID_Contig" boring="true">
    <s:stringconstant>mth2-26n6</s:stringconstant>
  </s:processor>
  <s:processor name="Flatten_l_l____to_l1">
    <s:local>org.embl.ebi.escience.scuflworkers.java.FlattenList</s:local>
  </s:processor>
  <s:processor name="Flatten_l_l____to_l">
    <s:local>org.embl.ebi.escience.scuflworkers.java.FlattenList</s:local>
  </s:processor>
  <s:processor name="Flatten_l_l____to_l3">
    <s:local>org.embl.ebi.escience.scuflworkers.java.FlattenList</s:local>
  </s:processor>
  <s:processor name="Flatten_l_l____to_l4">
    <s:local>org.embl.ebi.escience.scuflworkers.java.FlattenList</s:local>
  </s:processor>
  <s:processor name="Create_moby_data">
    <s:local>org.embl.ebi.escience.scuflworkers.biomoby.CreateMobyData</s:local>
  </s:processor>
  <s:processor name="Parse_moby_data1">
    <s:local>org.embl.ebi.escience.scuflworkers.biomoby.ExtractMobyData</s:local>
  </s:processor>
  <s:processor name="Parse_moby_data">
    <s:local>org.embl.ebi.escience.scuflworkers.biomoby.ExtractMobyData</s:local>
  </s:processor>
  <s:processor name="Parse_moby_data2">
    <s:local>org.embl.ebi.escience.scuflworkers.biomoby.ExtractMobyData</s:local>
  </s:processor>
  <s:processor name="MIPSBlastBetterE13">
    <s:description>executes blast against MAtDB Arabidopsis protein coding genes with a cutoff E-value of E=1e-13</s:description>
    <s:biomobywsdl>
      <s:mobyEndpoint>http://mobycentral.icapture.ubc.ca/cgi-bin/MOBY05/mobycentral.pl</s:mobyEndpoint>
      <s:serviceName>MIPSBlastBetterE13</s:serviceName>
      <s:authorityName>mips.gsf.de</s:authorityName>
    </s:biomobywsdl>
  </s:processor>
  <s:processor name="extractElementNames">
    <s:description>parses Element Names out of any text-xml or text-formatted (output sorted by alphabet)</s:description>
    <s:biomobywsdl>
      <s:mobyEndpoint>http://mobycentral.icapture.ubc.ca/cgi-bin/MOBY05/mobycentral.pl</s:mobyEndpoint>
      <s:serviceName>extractElementNames</s:serviceName>
      <s:authorityName>mips.gsf.de</s:authorityName>
    </s:biomobywsdl>
  </s:processor>
  <s:processor name="extractElementNames1">
    <s:description>parses Element Names out of any text-xml or text-formatted (output sorted by alphabet)</s:description>
    <s:biomobywsdl>
      <s:mobyEndpoint>http://mobycentral.icapture.ubc.ca/cgi-bin/MOBY05/mobycentral.pl</s:mobyEndpoint>
      <s:serviceName>extractElementNames</s:serviceName>
      <s:authorityName>mips.gsf.de</s:authorityName>
    </s:biomobywsdl>
  </s:processor>
  <s:processor name="extractElementSequences">
    <s:description>parses Element Sequences out of  any text-xml or text-formatted and gives them back as GenericSequences (output sorted by alphabet)</s:description>
    <s:biomobywsdl>
      <s:mobyEndpoint>http://mobycentral.icapture.ubc.ca/cgi-bin/MOBY05/mobycentral.pl</s:mobyEndpoint>
      <s:serviceName>extractElementSequences</s:serviceName>
      <s:authorityName>mips.gsf.de</s:authorityName>
    </s:biomobywsdl>
  </s:processor>
  <s:processor name="extractAGILocusCodes">
    <s:description>parses AGI-LocusCodes out of Blast Results or any text-xml or text-formatted  (output sorted by alphabet)</s:description>
    <s:biomobywsdl>
      <s:mobyEndpoint>http://mobycentral.icapture.ubc.ca/cgi-bin/MOBY05/mobycentral.pl</s:mobyEndpoint>
      <s:serviceName>extractAGILocusCodes</s:serviceName>
      <s:authorityName>mips.gsf.de</s:authorityName>
    </s:biomobywsdl>
  </s:processor>
  <s:processor name="getInterProAccByAGIcode">
    <s:description>Retrieves the Interpro accessions from AtEnsembl (http://atensembl.arabidopsis.info) for a given AGI-Code. (e.g. At2g38120)</s:description>
    <s:biomobywsdl>
      <s:mobyEndpoint>http://mobycentral.icapture.ubc.ca/cgi-bin/MOBY05/mobycentral.pl</s:mobyEndpoint>
      <s:serviceName>getInterProAccByAGIcode</s:serviceName>
      <s:authorityName>arabidopsis.info</s:authorityName>
    </s:biomobywsdl>
  </s:processor>
  <s:processor name="getGOAccsByAGICode">
    <s:description>takes an AGI code and returns a collection of GO accession numbers</s:description>
    <s:biomobywsdl>
      <s:mobyEndpoint>http://mobycentral.icapture.ubc.ca/cgi-bin/MOBY05/mobycentral.pl</s:mobyEndpoint>
      <s:serviceName>getGOAccsByAGICode</s:serviceName>
      <s:authorityName>atidb.org</s:authorityName>
    </s:biomobywsdl>
  </s:processor>
  <s:processor name="getElementsForContig">
    <s:description>retrieve all genetic elements for given contig as collection of plain-text objects</s:description>
    <s:biomobywsdl>
      <s:mobyEndpoint>http://mobycentral.icapture.ubc.ca/cgi-bin/MOBY05/mobycentral.pl</s:mobyEndpoint>
      <s:serviceName>getElementsForContig</s:serviceName>
      <s:authorityName>mips.gsf.de</s:authorityName>
    </s:biomobywsdl>
  </s:processor>
  <s:processor name="getContigsByFreeText">
    <s:description>parses Element Names out of any text-xml or text-formatted (output sorted by alphabet)</s:description>
    <s:biomobywsdl>
      <s:mobyEndpoint>http://mobycentral.icapture.ubc.ca/cgi-bin/MOBY05/mobycentral.pl</s:mobyEndpoint>
      <s:serviceName>getContigsByFreeText</s:serviceName>
      <s:authorityName>mips.gsf.de</s:authorityName>
    </s:biomobywsdl>
  </s:processor>
  <s:processor name="getProteinSequence">
    <s:description>Retrieves Protein sequence for a genetic element name</s:description>
    <s:biomobywsdl>
      <s:mobyEndpoint>http://mobycentral.icapture.ubc.ca/cgi-bin/MOBY05/mobycentral.pl</s:mobyEndpoint>
      <s:serviceName>getProteinSequence</s:serviceName>
      <s:authorityName>mips.gsf.de</s:authorityName>
    </s:biomobywsdl>
  </s:processor>
  <s:link source="Create_moby_data:mobydata" sink="getContigsByFreeText:input" />
  <s:link source="Flatten_l_l____to_l1:outputlist" sink="getProteinSequence:input" />
  <s:link source="Flatten_l_l____to_l3:outputlist" sink="MIPSBlastBetterE13:input" />
  <s:link source="Flatten_l_l____to_l4:outputlist" sink="Parse_moby_data:mobydata" />
  <s:link source="Flatten_l_l____to_l4:outputlist" sink="extractAGILocusCodes:input" />
  <s:link source="Flatten_l_l____to_l:outputlist" sink="getElementsForContig:input" />
  <s:link source="ID_Contig:value" sink="Create_moby_data:id" />
  <s:link source="MIPSBlastBetterE13:output" sink="Flatten_l_l____to_l4:inputlist" />
  <s:link source="NS_Contig:value" sink="Create_moby_data:namespace" />
  <s:link source="extractAGILocusCodes:output" sink="getGOAccsByAGICode:input" />
  <s:link source="extractAGILocusCodes:output" sink="getInterProAccByAGIcode:input" />
  <s:link source="extractElementSequences:output" sink="Flatten_l_l____to_l3:inputlist" />
  <s:link source="getContigsByFreeText:output" sink="extractElementNames:input" />
  <s:link source="getGOAccsByAGICode:output" sink="Parse_moby_data2:mobydata" />
  <s:link source="getInterProAccByAGIcode:output" sink="Parse_moby_data1:mobydata" />
  <s:link source="Parse_moby_data1:id" sink="InterproAccessions" />
  <s:link source="Parse_moby_data2:id" sink="GO_Accessions" />
  <s:link source="Parse_moby_data:value" sink="BlastResults" />
  <s:link source="extractElementNames1:output" sink="Flatten_l_l____to_l1:inputlist" />
  <s:link source="extractElementNames:output" sink="Flatten_l_l____to_l:inputlist" />
  <s:link source="getElementsForContig:output" sink="extractElementNames1:input" />
  <s:link source="getProteinSequence:output" sink="extractElementSequences:input" />
  <s:sink name="BlastResults" />
  <s:sink name="InterproAccessions" />
  <s:sink name="GO_Accessions" />
</s:scufl>


