  <scm>
    <!--<connection>scm:cvs:pserver:anonymous@cvs.sourceforge.net:/cvsroot/taverna:taverna1.0</connection>-->
    <connection>scm:cvs:ext:${sfusername}@cvs.sourceforge.net:/cvsroot/taverna:taverna1.0</connection>
    <developerConnection>scm:cvs:ext:${sfusername}@cvs.sourceforge.net:/cvsroot/taverna:taverna1.0</developerConnection>
    <url>http://cvs.sourceforge.net/viewcvs.py/taverna/taverna1.0/</url>
  </scm>
  <organization>
    <name>University of Manchester</name>
    <url>http://www.manchester.ac.uk</url>
  </organization>
  <build>
    <sourceDirectory>src</sourceDirectory>
    <testSourceDirectory>test</testSourceDirectory>
    <resources>
      <resource>
        <targetPath>/</targetPath>
        <directory>src</directory>
        <includes>
          <include>META-INF/**</include>
          <include>**/*.html</include>
          <include>**/*.gif</include>
          <include>**/*.png</include>
          <include>**/*.xml</include>
          <include>**/*.xsl</include>
          <include>**/*.properties</include>
        </includes>
      </resource>
    </resources>
    <testResources>
      <testResource>
        <targetPath>/</targetPath>
        <directory>workbench/conf</directory>
        <includes>
          <include>*.properties</include>
        </includes>
      </testResource>
    </testResources>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>1.4</source>
          <target>1.4</target>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-surefire-plugin</artifactId>
        <configuration>
          <skip>false</skip>
          <systemProperties>
            <property>
              <name>taverna.home</name>
              <value>${basedir}</value>
            </property>
          </systemProperties>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-assembly-plugin</artifactId>
        <configuration>
          <descriptor>assembly.xml</descriptor>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-antrun-plugin</artifactId>
        <executions>
          <execution>
            <id>jreleaseinfo</id>
            <phase>generate-sources</phase>
            <configuration>
              <tasks>
                <taskdef name="jreleaseinfo" classname="ch.oscg.jreleaseinfo.anttask.JReleaseInfoAntTask">
                  <classpath>
                    <pathelement location="devlib/jreleaseinfo-1.2.0.jar"/>
                  </classpath>
                </taskdef>
                <jreleaseinfo targetDir="${project.build.directory}/generated-sources/jreleaseinfo"
                              className="TavernaReleaseInfo"
                              packageName="org.embl.ebi.escience.scufl"
                              project="${artifactId}"
                              version="${version}"
                              withViewer="false"/>
              </tasks>
              <sourceRoot>${project.build.directory}/generated-sources/jreleaseinfo</sourceRoot>
            </configuration>
            <goals>
              <goal>run</goal>
            </goals>
         </execution>
          <execution>
            <id>launcher</id>
            <phase>package</phase>
            <configuration>
              <tasks>
                <jar destfile="${project.build.directory}/${project.build.finalName}-launcher.jar"
                     basedir="${project.build.outputDirectory}"
                     includes="**/WorkbenchLauncher*.class"/>
              </tasks>
            </configuration>
            <goals>
              <goal>run</goal>
            </goals>
         </execution>
        </executions>
      </plugin>
    </plugins>
  </build>
