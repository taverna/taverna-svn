<?xml version="1.0" encoding="UTF-8"?>
<s:scufl xmlns:s="http://org.embl.ebi.escience/xscufl/0.1alpha" version="0.1" log="3">
  <s:processor name="databaseid">
    <s:description>A identifier to a database indexed by SRS which is to be cross-referenced using SRS links</s:description>
    <s:stringconstant>MEDLINE</s:stringconstant>
  </s:processor>
  <s:processor name="srsebi_getmedlineid">
    <s:description>Retrieves the MEDLINE Ids associated with a given EMBL accession number using SRS links</s:description>
    <s:arbitrarywsdl>
      <s:wsdl>http://mygrid.ncl.ac.uk/axis/services/SrsEbiQuery?wsdl</s:wsdl>
      <s:operation>queryByXRef</s:operation>
    </s:arbitrarywsdl>
  </s:processor>
  <s:processor name="srsebi_getmedlinerecords">
    <s:description>Retrieves the MEDLINE records associated with a array of MEDLINE Ids</s:description>
    <s:arbitrarywsdl>
      <s:wsdl>http://mygrid.ncl.ac.uk/axis/services/SrsEbiQuery?wsdl</s:wsdl>
      <s:operation>queryByArrayIds</s:operation>
    </s:arbitrarywsdl>
  </s:processor>
  <s:processor name="affymapper_getemblaccnumber">
    <s:description>Retrieves the EMBL accession number associated with a given Affymetrix probe set Id</s:description>
    <s:arbitrarywsdl>
      <s:wsdl>http://mygrid.ncl.ac.uk/axis/services/affymapper?wsdl</s:wsdl>
      <s:operation>getAccessionNumber</s:operation>
    </s:arbitrarywsdl>
  </s:processor>
  <s:processor name="probeSetId">
    <s:stringconstant>134_at</s:stringconstant>
  </s:processor>
  <s:link>
    <s:input>srsebi_getmedlineid:usa</s:input>
    <s:output>affymapper_getemblaccnumber:getAccessionNumberReturn</s:output>
  </s:link>
  <s:link>
    <s:input>srsebi_getmedlineid:XRefDatabankId</s:input>
    <s:output>databaseid:value</s:output>
  </s:link>
  <s:link>
    <s:input>srsebi_getmedlinerecords:usas</s:input>
    <s:output>srsebi_getmedlineid:queryByXRefReturn</s:output>
  </s:link>
  <s:link>
    <s:input>medlinerecords</s:input>
    <s:output>srsebi_getmedlinerecords:queryByArrayIdsReturn</s:output>
  </s:link>
  <s:link>
    <s:input>affymapper_getemblaccnumber:probeSetId</s:input>
    <s:output>probeSetId:value</s:output>
  </s:link>
  <s:sink>
    medlinerecords
    <s:metadata>
      <s:mimeTypes />
      <s:description>An array of MEDLINE records</s:description>
      <s:semanticType>http://www.mygrid.org.uk/ontology#bioinformatics_record</s:semanticType>
    </s:metadata>
  </s:sink>
</s:scufl>


