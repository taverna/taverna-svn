<?xml version="1.0" encoding="UTF-8"?>
<s:scufl xmlns:s="http://org.embl.ebi.escience/xscufl/0.1alpha" version="0.1" log="0">
  <s:processor name="isbn_default">
    <s:stringconstant>0596002246</s:stringconstant>
  </s:processor>
  <s:processor name="xmethods_book_price">
    <s:arbitrarywsdl>
      <s:wsdl>http://www.xmethods.net/sd/2001/BNQuoteService.wsdl</s:wsdl>
      <s:operation>getPrice</s:operation>
    </s:arbitrarywsdl>
  </s:processor>
  <s:link>
    <s:input>xmethods_book_price:isbn</s:input>
    <s:output>isbn_default:value</s:output>
  </s:link>
  <s:link>
    <s:input>price_dollars</s:input>
    <s:output>xmethods_book_price:return</s:output>
  </s:link>
  <s:sink>
    price_dollars
    <s:metadata>
      <s:mimeTypes />
      <s:description />
      <s:semanticType />
    </s:metadata>
  </s:sink>
</s:scufl>


