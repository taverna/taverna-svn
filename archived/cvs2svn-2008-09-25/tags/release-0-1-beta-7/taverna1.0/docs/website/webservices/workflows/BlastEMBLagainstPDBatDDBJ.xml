<?xml version="1.0" encoding="UTF-8"?>
<s:scufl xmlns:s="http://org.embl.ebi.escience/xscufl/0.1alpha" version="0.1" log="3">
  <s:processor name="blast_program">
    <s:stringconstant>blastx</s:stringconstant>
  </s:processor>
  <s:processor name="blast_db">
    <s:stringconstant>PDB</s:stringconstant>
  </s:processor>
  <s:processor name="example_sequenceID">
    <s:description>This string constant should be a sequence_usa as requires by the soaplab seqret service, 
e.g. embl-AccNumber:X07024, or embl:xlrhodop</s:description>
    <s:stringconstant>embl:U12466</s:stringconstant>
  </s:processor>
  <s:processor name="blastx_ddbj">
    <s:description>Blast service at XML Central of DDBJ http://xml.nig.ac.jp/wsdl/index.jsp
Documentation http://xml.nig.ac.jp/doc/Blast.txt</s:description>
    <s:arbitrarywsdl>
      <s:wsdl>http://xml.nig.ac.jp/wsdl/Blast.wsdl</s:wsdl>
      <s:operation>searchSimple</s:operation>
    </s:arbitrarywsdl>
  </s:processor>
  <s:processor name="get_sequence">
    <s:description>Reads and writes (returns) sequences</s:description>
    <s:soaplabwsdl>http://industry.ebi.ac.uk/soap/soaplab/edit::seqret</s:soaplabwsdl>
  </s:processor>
  <s:link>
    <s:input>outfile_SeqretTest</s:input>
    <s:output>get_sequence:outseq</s:output>
  </s:link>
  <s:link>
    <s:input>get_sequence:sequence_usa</s:input>
    <s:output>example_sequenceID:value</s:output>
  </s:link>
  <s:link>
    <s:input>blastx_ddbj:query</s:input>
    <s:output>get_sequence:outseq</s:output>
  </s:link>
  <s:link>
    <s:input>blastx_ddbj:program</s:input>
    <s:output>blast_program:value</s:output>
  </s:link>
  <s:link>
    <s:input>blastx_ddbj:database</s:input>
    <s:output>blast_db:value</s:output>
  </s:link>
  <s:link>
    <s:input>blast_result_ddbj</s:input>
    <s:output>blastx_ddbj:Result</s:output>
  </s:link>
  <s:sink>
    outfile_SeqretTest
    <s:metadata>
      <s:mimeTypes />
      <s:description>Output from seqret - exact semantic type will depend on inputs</s:description>
      <s:semanticType>http://www.mygrid.org.uk/ontology#nucleotide_sequence_data</s:semanticType>
    </s:metadata>
  </s:sink>
  <s:sink>
    blast_result_ddbj
    <s:metadata>
      <s:mimeTypes>
        <s:mimeType>text/plain</s:mimeType>
      </s:mimeTypes>
      <s:description>BLAST report</s:description>
      <s:semanticType />
    </s:metadata>
  </s:sink>
</s:scufl>


