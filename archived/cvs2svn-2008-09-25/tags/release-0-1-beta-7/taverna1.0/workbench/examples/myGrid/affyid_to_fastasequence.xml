<?xml version="1.0" encoding="UTF-8"?>
<s:scufl xmlns:s="http://org.embl.ebi.escience/xscufl/0.1alpha" version="0.1" log="3">
  <s:processor name="affymapper_getseq">
    <s:description>Retrieves the nucleotide sequence associated with a given Affymetrix probe set Id</s:description>
    <s:arbitrarywsdl>
      <s:wsdl>http://mygrid.ncl.ac.uk/axis/services/affymapper?wsdl</s:wsdl>
      <s:operation>getSequence</s:operation>
    </s:arbitrarywsdl>
  </s:processor>
  <s:processor name="probeSetId">
    <s:stringconstant>1001_at</s:stringconstant>
  </s:processor>
  <s:link>
    <s:input>sequence</s:input>
    <s:output>affymapper_getseq:getSequenceReturn</s:output>
  </s:link>
  <s:link>
    <s:input>affymapper_getseq:probeSetId</s:input>
    <s:output>probeSetId:value</s:output>
  </s:link>
  <s:sink>
    sequence
    <s:metadata>
      <s:mimeTypes>
        <s:mimeType>text/plain</s:mimeType>
      </s:mimeTypes>
      <s:description>Nucleotide sequence associated with Affymetrix probe set Id in FASTA format</s:description>
      <s:semanticType>http://www.mygrid.org.uk/ontology#nucleotide_sequence_data</s:semanticType>
    </s:metadata>
  </s:sink>
</s:scufl>


