<?xml version="1.0" encoding="UTF-8"?>
<s:scufl xmlns:s="http://org.embl.ebi.escience/xscufl/0.1alpha" version="0.2" log="0">
  <s:workflowdescription lsid="urn:lsid:www.mygrid.org.uk:operation:FLTYJHK2Y30" author="" title="" />
  <s:processor name="b" boring="true">
    <s:stringconstant>b</s:stringconstant>
  </s:processor>
  <s:processor name="String_list_union">
    <s:local>org.embl.ebi.escience.scuflworkers.java.StringSetUnion</s:local>
  </s:processor>
  <s:processor name="a" boring="true">
    <s:stringconstant>a</s:stringconstant>
  </s:processor>
  <s:processor name="Concatenate_two_strings">
    <s:local>org.embl.ebi.escience.scuflworkers.java.StringConcat</s:local>
    <s:iterationstrategy>
      <i:cross xmlns:i="http://org.embl.ebi.escience/xscufliteration/0.1beta10">
        <i:iterator name="string1" />
        <i:iterator name="string2" />
      </i:cross>
    </s:iterationstrategy>
  </s:processor>
  <s:processor name="Merge_string_list_to_string">
    <s:local>org.embl.ebi.escience.scuflworkers.java.StringListMerge</s:local>
  </s:processor>
  <s:link source="String_list_union:union" sink="Concatenate_two_strings:string1" />
  <s:link source="String_list_union:union" sink="Concatenate_two_strings:string2" />
  <s:link source="String_list_union:union" sink="Merge_string_list_to_string:stringlist" />
  <s:link source="a:value" sink="String_list_union:list1" />
  <s:link source="b:value" sink="Merge_string_list_to_string:seperator" />
  <s:link source="Concatenate_two_strings:output" sink="out" />
  <s:link source="Merge_string_list_to_string:concatenated" sink="out2" />
  <s:link source="b:value" sink="String_list_union:list2" />
  <s:sink name="out" />
  <s:sink name="out2" />
  <s:coordination name="Concatenate_two_strings_BLOCKON_Merge_string_list_to_string">
    <s:condition>
      <s:state>Completed</s:state>
      <s:target>Merge_string_list_to_string</s:target>
    </s:condition>
    <s:action>
      <s:target>Concatenate_two_strings</s:target>
      <s:statechange>
        <s:from>Scheduled</s:from>
        <s:to>Running</s:to>
      </s:statechange>
    </s:action>
  </s:coordination>
</s:scufl>


