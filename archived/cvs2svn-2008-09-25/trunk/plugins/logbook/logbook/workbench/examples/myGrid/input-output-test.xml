<?xml version="1.0" encoding="UTF-8"?>
<s:scufl xmlns:s="http://org.embl.ebi.escience/xscufl/0.1alpha" version="0.2" log="0">
  <s:workflowdescription lsid="urn:lsid:www.mygrid.org.uk:operation:HARAZWC8RH0" author="Daniele Turi" title="Example IO workflow" />
  <s:processor name="Concatenate_two_strings">
    <s:local>org.embl.ebi.escience.scuflworkers.java.StringConcat</s:local>
  </s:processor>
  <s:link source="string1" sink="Concatenate_two_strings:string1" />
  <s:link source="string2" sink="Concatenate_two_strings:string2" />
  <s:link source="Concatenate_two_strings:output" sink="output_test" />
  <s:source name="string2" />
  <s:source name="string1" />
  <s:sink name="output_test" />
</s:scufl>

