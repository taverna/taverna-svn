<plugin>
	<name>Proxy Manager</name>
	<description>Assists in creating GridProxy, MyProxy Credentials</description>
	<identifier>net.sf.taverna</identifier>
	<version>1.0.0</version>
	<provider>net.sf.taverna.credential</provider>
	<repositories>
		<repository>
			http://www.mygrid.org.uk/maven/repository
		</repository>
	</repositories>
	<profile>
		<artifact groupId="net.sf.taverna.credential" artifactId="proxy-plugin"
			version="0.1.0" />
	</profile>
	<taverna>
		<version>1.7</version>
	</taverna>
</plugin>
