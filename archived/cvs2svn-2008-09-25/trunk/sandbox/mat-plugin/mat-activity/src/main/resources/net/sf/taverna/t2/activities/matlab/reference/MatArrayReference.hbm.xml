<?xml version="1.0" ?>
<!DOCTYPE hibernate-mapping PUBLIC
                "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- Hibernate mapping for MatArray reference bean -->
<hibernate-mapping>
	<joined-subclass
		name="net.sf.taverna.t2.activities.matlab.reference.MatArrayReference"
		extends="net.sf.taverna.t2.reference.AbstractExternalReference">
		<!-- Link to primary key from abstract superclass -->
		<key column="bean_id" />
		<!-- HttpReference specific properties below here -->
		<property name="contents" type="string" />
	</joined-subclass>
</hibernate-mapping>