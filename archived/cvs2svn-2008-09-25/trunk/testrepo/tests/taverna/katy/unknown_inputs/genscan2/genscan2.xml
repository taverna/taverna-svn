<?xml version="1.0" encoding="UTF-8"?>
<s:scufl xmlns:s="http://org.embl.ebi.escience/xscufl/0.1alpha" version="0.2" log="0">
  <s:workflowdescription lsid="urn:lsid:www.mygrid.org.uk:operation:OBRWZP2ZPF43" author="" title="" />
  <s:processor name="searchSimple">
    <s:arbitrarywsdl>
      <s:wsdl>http://xml.nig.ac.jp/wsdl/Blast.wsdl</s:wsdl>
      <s:operation>searchSimple</s:operation>
    </s:arbitrarywsdl>
  </s:processor>
  <s:processor name="lister">
    <s:arbitrarywsdl>
      <s:wsdl>http://phoebus.cs.man.ac.uk:8081/axis/FastaLister.jws?wsdl</s:wsdl>
      <s:operation>lister</s:operation>
    </s:arbitrarywsdl>
  </s:processor>
  <s:processor name="database" boring="true">
    <s:stringconstant>SWISS</s:stringconstant>
  </s:processor>
  <s:processor name="program" boring="true">
    <s:stringconstant>blastp</s:stringconstant>
  </s:processor>
  <s:processor name="genscan">
    <s:description>Runs genscan on the input sequence</s:description>
    <s:soaplabwsdl>http://phoebus.cs.man.ac.uk:8081/axis/services/seq_analysis.genscan</s:soaplabwsdl>
  </s:processor>
  <s:processor name="genscansplitter">
    <s:description>Runs genscan on the input sequence</s:description>
    <s:soaplabwsdl>http://phoebus.cs.man.ac.uk:8081/axis/services/seq_analysis.genscansplitter</s:soaplabwsdl>
  </s:processor>
  <s:processor name="hmoment">
    <s:description>Hydrophobic moment calculation</s:description>
    <s:soaplabwsdl>http://www.ebi.ac.uk/soaplab/services/protein_2d_structure.hmoment</s:soaplabwsdl>
  </s:processor>
  <s:processor name="garnier">
    <s:description>Predicts protein secondary structure</s:description>
    <s:soaplabwsdl>http://www.ebi.ac.uk/soaplab/services/protein_2d_structure.garnier</s:soaplabwsdl>
  </s:processor>
  <s:processor name="pepcoil">
    <s:description>Predicts coiled coil regions</s:description>
    <s:soaplabwsdl>http://www.ebi.ac.uk/soaplab/services/protein_2d_structure.pepcoil</s:soaplabwsdl>
  </s:processor>
  <s:processor name="pscan">
    <s:description>Scans proteins using PRINTS</s:description>
    <s:soaplabwsdl>http://www.ebi.ac.uk/soaplab/services/protein_motifs.pscan</s:soaplabwsdl>
  </s:processor>
  <s:processor name="patmatmotifs">
    <s:description>Search a PROSITE motif database with a
                  protein sequence</s:description>
    <s:soaplabwsdl>http://www.ebi.ac.uk/soaplab/services/protein_motifs.patmatmotifs</s:soaplabwsdl>
  </s:processor>
  <s:processor name="helixturnhelix">
    <s:description>Report nucleic acid binding motifs</s:description>
    <s:soaplabwsdl>http://www.ebi.ac.uk/soaplab/services/protein_2d_structure.helixturnhelix</s:soaplabwsdl>
  </s:processor>
  <s:processor name="tmap">
    <s:description>Displays membrane spanning regions</s:description>
    <s:soaplabwsdl>http://www.ebi.ac.uk/soaplab/services/protein_2d_structure.tmap</s:soaplabwsdl>
  </s:processor>
  <s:processor name="proteinraw">
    <s:description>InterProScan with output in TXT format, for protein</s:description>
    <s:soaplabwsdl>http://www.ebi.ac.uk/soaplab/services/interproscan.proteinraw</s:soaplabwsdl>
  </s:processor>
  <s:link source="DNAsequence" sink="genscan:sequence_direct_data" />
  <s:link source="database:value" sink="searchSimple:database" />
  <s:link source="genscan:output" sink="genscansplitter:scanrecord_direct_data" />
  <s:link source="lister:listerReturn" sink="pepcoil:sequence_direct_data" />
  <s:link source="garnier:outfile" sink="prot_2d_structure" />
  <s:link source="genscansplitter:coding" sink="coding output" />
  <s:link source="genscansplitter:peptide" sink="lister:file" />
  <s:link source="genscansplitter:report" sink="other" />
  <s:link source="genscansplitter:scanreport" sink="genscan_report" />
  <s:link source="helixturnhelix:outfile" sink="Nuc_acid_binding" />
  <s:link source="hmoment:outfile" sink="hydrophobic_moment" />
  <s:link source="lister:listerReturn" sink="garnier:sequence_direct_data" />
  <s:link source="lister:listerReturn" sink="helixturnhelix:sequence_direct_data" />
  <s:link source="lister:listerReturn" sink="hmoment:seqall_direct_data" />
  <s:link source="lister:listerReturn" sink="patmatmotifs:sequence_direct_data" />
  <s:link source="lister:listerReturn" sink="pscan:sequence_direct_data" />
  <s:link source="lister:listerReturn" sink="tmap:sequences_direct_data" />
  <s:link source="lister:listerReturn" sink="predicted_peptide" />
  <s:link source="lister:listerReturn" sink="proteinraw:sequence" />
  <s:link source="lister:listerReturn" sink="searchSimple:query" />
  <s:link source="patmatmotifs:outfile" sink="Prosite_results" />
  <s:link source="pepcoil:outfile" sink="Coiled_regions" />
  <s:link source="program:value" sink="searchSimple:program" />
  <s:link source="proteinraw:result" sink="Interpro_results" />
  <s:link source="pscan:outfile" sink="Prints_results" />
  <s:link source="searchSimple:Result" sink="blast_results" />
  <s:link source="tmap:Graphics_in_PNG" sink="membrane_span_graphic" />
  <s:link source="tmap:outfile" sink="membrane_spanning" />
  <s:source name="DNAsequence">
    <s:metadata>
      <s:mimeTypes>
        <s:mimeType>text/xml</s:mimeType>
      </s:mimeTypes>
    </s:metadata>
  </s:source>
  <s:sink name="coding output" />
  <s:sink name="genscan_report" />
  <s:sink name="other" />
  <s:sink name="predicted_peptide" />
  <s:sink name="blast_results" />
  <s:sink name="Coiled_regions" />
  <s:sink name="hydrophobic_moment" />
  <s:sink name="prot_2d_structure" />
  <s:sink name="Prints_results" />
  <s:sink name="Prosite_results" />
  <s:sink name="Nuc_acid_binding" />
  <s:sink name="membrane_spanning" />
  <s:sink name="Interpro_results" />
  <s:sink name="membrane_span_graphic">
    <s:metadata>
      <s:mimeTypes>
        <s:mimeType>image/png</s:mimeType>
      </s:mimeTypes>
    </s:metadata>
  </s:sink>
</s:scufl>





