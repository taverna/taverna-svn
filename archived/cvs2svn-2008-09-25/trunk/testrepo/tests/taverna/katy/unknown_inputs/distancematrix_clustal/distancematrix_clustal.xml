<?xml version="1.0" encoding="UTF-8"?>
<s:scufl xmlns:s="http://org.embl.ebi.escience/xscufl/0.1alpha" version="0.2" log="0">
  <s:workflowdescription lsid="urn:lsid:www.mygrid.org.uk:operation:H0QMOP816X24" author="" title="" />
  <s:processor name="distmat">
    <s:description>Creates a distance matrix from multiple
                  alignments</s:description>
    <s:soaplabwsdl>http://www.ebi.ac.uk/soaplab/services/phylogeny_distance_matrix.distmat</s:soaplabwsdl>
  </s:processor>
  <s:processor name="prettyplot">
    <s:description>Displays aligned sequences, with colouring
                  and boxing</s:description>
    <s:soaplabwsdl>http://www.ebi.ac.uk/soaplab/services/alignment_multiple.prettyplot</s:soaplabwsdl>
  </s:processor>
  <s:link source="proteins" sink="distmat:sequence_direct_data" />
  <s:link source="proteins" sink="prettyplot:sequences_direct_data" />
  <s:link source="distmat:outfile" sink="distance_matrix" />
  <s:link source="prettyplot:Graphics_in_PNG" sink="pngGraphic" />
  <s:link source="prettyplot:report" sink="report" />
  <s:source name="proteins" />
  <s:sink name="report" />
  <s:sink name="pngGraphic">
    <s:metadata>
      <s:mimeTypes>
        <s:mimeType>image/png</s:mimeType>
      </s:mimeTypes>
    </s:metadata>
  </s:sink>
  <s:sink name="distance_matrix" />
</s:scufl>





