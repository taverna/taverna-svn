<?xml version="1.0" encoding="UTF-8"?>
<s:scufl xmlns:s="http://org.embl.ebi.escience/xscufl/0.1alpha" version="0.2" log="0">
  <s:processor name="Blast2_program">
    <s:stringconstant>blastn</s:stringconstant>
  </s:processor>
  <s:processor name="comparer">
    <s:description>Compares a new BLAST output with an older output to check for new hits</s:description>
    <s:soaplabwsdl>http://phoebus.cs.man.ac.uk:8081/axis/services/seq_analysis::blastcomparer</s:soaplabwsdl>
  </s:processor>
  <s:processor name="ncbiblast">
    <s:description>Wrapper for BLAST at NCBI</s:description>
    <s:soaplabwsdl>http://phoebus.cs.man.ac.uk:8081/axis/services/seq_analysis::ncbiblastwrapper</s:soaplabwsdl>
  </s:processor>
  <s:processor name="Fasta_to_numbered">
    <s:description>converts fasta seq into numbered genbank format</s:description>
    <s:soaplabwsdl>http://phoebus.cs.man.ac.uk:8081/axis/services/seq_analysis::fasta_to_numbered</s:soaplabwsdl>
  </s:processor>
  <s:processor name="repeatmasker">
    <s:description>Screens for low complexity DNA sequences and interspersed repeats</s:description>
    <s:soaplabwsdl>http://phoebus.cs.man.ac.uk:8081/axis/services/seq_analysis::repeatmasker</s:soaplabwsdl>
  </s:processor>
  <s:processor name="simplifier">
    <s:description>Simpliies BLAST output for later comparison</s:description>
    <s:soaplabwsdl>http://phoebus.cs.man.ac.uk:8081/axis/services/seq_analysis::blastsimplifier</s:soaplabwsdl>
  </s:processor>
  <s:processor name="retrieve">
    <s:description>Retrieves Genbank records given the blast comparison report</s:description>
    <s:soaplabwsdl>http://phoebus.cs.man.ac.uk:8081/axis/services/seq_analysis::genbankretrieve</s:soaplabwsdl>
  </s:processor>
  <s:processor name="blast2">
    <s:description>Compares two blast outputs</s:description>
    <s:soaplabwsdl>http://phoebus.cs.man.ac.uk:8081/axis/services/seq_analysis::blast2</s:soaplabwsdl>
  </s:processor>
  <s:processor name="lister">
    <s:arbitrarywsdl>
      <s:wsdl>http://phoebus.cs.man.ac.uk:8081/axis/FastaLister.jws?wsdl</s:wsdl>
      <s:operation>lister</s:operation>
    </s:arbitrarywsdl>
  </s:processor>
  <s:processor name="copyright">
    <s:description>script to generate copyright statement</s:description>
    <s:soaplabwsdl>http://phoebus.cs.man.ac.uk:8081/axis/services/documentation::copyright</s:soaplabwsdl>
  </s:processor>
  <s:link source="repeatmasker:masked" sink="ncbiblast:query_file_direct_data" />
  <s:link source="ncbiblast:output" sink="simplifier:blast_direct_data" />
  <s:link source="simplifier:output" sink="comparer:blastnew_direct_data" />
  <s:link source="old_result" sink="comparer:blastold_direct_data" />
  <s:link source="simplifier:output" sink="simple" />
  <s:link source="comparer:output" sink="comparison" />
  <s:link source="comparer:output" sink="retrieve:comparereport_direct_data" />
  <s:link source="bare_seq_in" sink="repeatmasker:sequence_direct_data" />
  <s:link source="retrieve:hitfilegenbank" sink="genbank" />
  <s:link source="retrieve:hitfilefasta" sink="fasta_out" />
  <s:link source="retrieve:missed" sink="missed" />
  <s:link source="retrieve:hitfilefasta" sink="Fasta_to_numbered:fasta_direct_data" />
  <s:link source="Fasta_to_numbered:numberedseq" sink="Seq_numbered" />
  <s:link source="species" sink="retrieve:species" />
  <s:link source="chromosome" sink="retrieve:chromosome" />
  <s:link source="bare_seq_in" sink="blast2:sequence1_direct_data" />
  <s:link source="Blast2_program:value" sink="blast2:program" />
  <s:link source="blast2:output" sink="B2_Alignment" />
  <s:link source="retrieve:hitfilefasta" sink="lister:file" />
  <s:link source="lister:listerReturn" sink="blast2:sequence2_direct_data" />
  <s:link source="copyright:out" sink="COPYRIGHT" />
  <s:source>
    bare_seq_in
    <s:metadata>
      <s:mimeTypes>
        <s:mimeType>text/plain</s:mimeType>
      </s:mimeTypes>
    </s:metadata>
  </s:source>
  <s:source>
    old_result
    <s:metadata>
      <s:mimeTypes>
        <s:mimeType>text/plain</s:mimeType>
      </s:mimeTypes>
    </s:metadata>
  </s:source>
  <s:source>
    species
    <s:metadata>
      <s:mimeTypes>
        <s:mimeType>text/plain</s:mimeType>
      </s:mimeTypes>
    </s:metadata>
  </s:source>
  <s:source>
    chromosome
    <s:metadata>
      <s:mimeTypes>
        <s:mimeType>text/plain</s:mimeType>
      </s:mimeTypes>
    </s:metadata>
  </s:source>
  <s:sink>
    simple
    <s:metadata>
      <s:mimeTypes>
        <s:mimeType>text/plain</s:mimeType>
      </s:mimeTypes>
    </s:metadata>
  </s:sink>
  <s:sink>
    comparison
    <s:metadata>
      <s:mimeTypes>
        <s:mimeType>text/plain</s:mimeType>
      </s:mimeTypes>
    </s:metadata>
  </s:sink>
  <s:sink>
    genbank
    <s:metadata>
      <s:mimeTypes>
        <s:mimeType>text/plain</s:mimeType>
      </s:mimeTypes>
    </s:metadata>
  </s:sink>
  <s:sink>
    fasta_out
    <s:metadata>
      <s:mimeTypes>
        <s:mimeType>text/plain</s:mimeType>
      </s:mimeTypes>
    </s:metadata>
  </s:sink>
  <s:sink>
    missed
    <s:metadata>
      <s:mimeTypes>
        <s:mimeType>text/plain</s:mimeType>
      </s:mimeTypes>
    </s:metadata>
  </s:sink>
  <s:sink>
    Seq_numbered
    <s:metadata>
      <s:mimeTypes>
        <s:mimeType>text/plain</s:mimeType>
      </s:mimeTypes>
    </s:metadata>
  </s:sink>
  <s:sink>
    B2_Alignment
    <s:metadata>
      <s:mimeTypes>
        <s:mimeType>text/plain</s:mimeType>
      </s:mimeTypes>
    </s:metadata>
  </s:sink>
  <s:sink>
    COPYRIGHT
    <s:metadata>
      <s:mimeTypes>
        <s:mimeType>text/plain</s:mimeType>
      </s:mimeTypes>
    </s:metadata>
  </s:sink>
</s:scufl>


