<?xml version="1.0" encoding="UTF-8"?>
<s:scufl xmlns:s="http://org.embl.ebi.escience/xscufl/0.1alpha" version="0.1" log="0">
  <s:processor name="wbidas">
    <s:arbitrarywsdl>
      <s:wsdl>http://www.vbi.vt.edu/~pathport/services/wsdls/vbidas.wsdl</s:wsdl>
      <s:operation>getOrganisms</s:operation>
    </s:arbitrarywsdl>
  </s:processor>
  <s:link>
    <s:input>genome_info</s:input>
    <s:output>wbidas:getOrganismsReturn</s:output>
  </s:link>
  <s:sink>
    genome_info
    <s:metadata>
      <s:mimeTypes />
      <s:description>Information about the genomes in the RefSeq database installed at the Viginia Bioinformatics Institute</s:description>
      <s:semanticType>http://www.mygrid.org.uk/ontology#bioinformatics_domain_concept</s:semanticType>
    </s:metadata>
  </s:sink>
</s:scufl>


