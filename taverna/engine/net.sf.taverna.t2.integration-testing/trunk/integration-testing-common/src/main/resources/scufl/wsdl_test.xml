<?xml version="1.0" encoding="UTF-8"?>
<s:scufl xmlns:s="http://org.embl.ebi.escience/xscufl/0.1alpha" version="0.2" log="0">
  <s:workflowdescription lsid="urn:lsid:www.mygrid.org.uk:operation:AJUERQ0C210" author="" title="Get Pathways by Gene" />
  <s:processor name="get_pathways_by_genes">
    <s:arbitrarywsdl>
      <s:wsdl>http://soap.genome.jp/KEGG.wsdl</s:wsdl>
      <s:operation>get_pathways_by_genes</s:operation>
    </s:arbitrarywsdl>
  </s:processor>
  <s:processor name="Make_gene_list">
    <s:beanshell>
      <s:scriptvalue>List gene_list=new ArrayList();
gene_list.add("eco:b0077");</s:scriptvalue>
      <s:beanshellinputlist />
      <s:beanshelloutputlist>
        <s:beanshelloutput s:syntactictype="l('text/plain')">gene_list</s:beanshelloutput>
      </s:beanshelloutputlist>
      <s:dependencies s:classloader="iteration" />
    </s:beanshell>
  </s:processor>
  <s:link source="Make_gene_list:gene_list" sink="get_pathways_by_genes:genes_id_list" />
  <s:link source="get_pathways_by_genes:return" sink="out" />
  <s:sink name="out" />
</s:scufl>

