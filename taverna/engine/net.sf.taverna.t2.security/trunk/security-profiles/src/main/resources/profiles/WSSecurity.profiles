
-----BEGIN PROFILE-----
Name=Timestamp
Description=This profile adds a timestamp header to the message.
Profile=<deployment xmlns="http://xml.apache.org/axis/wsdd/" 
		xmlns:java="http://xml.apache.org/axis/wsdd/providers/java"> 
		<globalConfiguration>
		<requestFlow>
		<handler type="java:net.sf.taverna.t2.activities.wsdl.wss4j.T2WSDoAllSender"> 
		<parameter name="action" value="Timestamp"/>
		</handler> 
		</requestFlow> 
		</globalConfiguration> 
		<transport name="http" pivot="java:org.apache.axis.transport.http.HTTPSender"/> 
		</deployment>		
-----END PROFILE-----

